{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.map.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      formData: {\n        nama: \"\",\n        email: \"\",\n        kamar_id: \"\",\n        periode_penyewaan: 1,\n        awal_penyewaan: \"\",\n        akhir_penyewaan: \"\",\n        jumlah_harga: 3000000,\n        bukti_pembayaran: null\n      },\n      kamarTersedia: [],\n      notification: {\n        message: \"\",\n        type: \"\"\n      }\n    };\n  },\n  methods: {\n    async fetchKamarTersedia() {\n      try {\n        const response = await axios.get(\"http://127.0.0.1:5000/kamar/tersedia\");\n        this.kamarTersedia = response.data.map(kamar => ({\n          id: kamar.id,\n          nomor_kamar: kamar.nomor_kamar\n        }));\n      } catch (error) {\n        this.notification = {\n          message: \"Gagal memuat data kamar.\",\n          type: \"error\"\n        };\n      }\n    },\n    updateJumlahHarga() {\n      this.formData.jumlah_harga = this.formData.periode_penyewaan * 3000000;\n    },\n    updateAkhirPenyewaan() {\n      const awal = new Date(this.formData.awal_penyewaan);\n      const akhir = new Date(awal);\n      akhir.setMonth(awal.getMonth() + parseInt(this.formData.periode_penyewaan, 10));\n      this.formData.akhir_penyewaan = akhir.toISOString().split(\"T\")[0];\n    },\n    handleFileUpload(event) {\n      this.formData.bukti_pembayaran = event.target.files[0];\n    },\n    async submitReservasi() {\n      const formDataToSend = new FormData();\n      for (const [key, value] of Object.entries(this.formData)) {\n        formDataToSend.append(key, value);\n      }\n      try {\n        const response = await axios.post(\"http://127.0.0.1:5000/reservasi\", formDataToSend, {\n          headers: {\n            \"Content-Type\": \"multipart/form-data\"\n          }\n        });\n        this.notification = {\n          message: response.data.message,\n          type: \"success\"\n        };\n        this.$router.push(\"/\");\n      } catch (error) {\n        this.notification = {\n          message: error.response.data.error || \"Gagal mengirim data reservasi.\",\n          type: \"error\"\n        };\n      }\n    }\n  },\n  mounted() {\n    this.fetchKamarTersedia();\n  }\n};","map":{"version":3,"names":["axios","data","formData","nama","email","kamar_id","periode_penyewaan","awal_penyewaan","akhir_penyewaan","jumlah_harga","bukti_pembayaran","kamarTersedia","notification","message","type","methods","fetchKamarTersedia","response","get","map","kamar","id","nomor_kamar","error","updateJumlahHarga","updateAkhirPenyewaan","awal","Date","akhir","setMonth","getMonth","parseInt","toISOString","split","handleFileUpload","event","target","files","submitReservasi","formDataToSend","FormData","key","value","Object","entries","append","post","headers","$router","push","mounted"],"sources":["D:\\SEMESTER 5\\Proyek\\Web_Kost\\frontend\\src\\views\\ReservasiKamar.vue"],"sourcesContent":["<template>\r\n  <div class=\"content\">\r\n    <div class=\"col-md-8\">\r\n      <div class=\"card card-user\">\r\n        <div class=\"card-header\">\r\n          <h5 class=\"card-title\">Form Reservasi Kamar</h5>\r\n        </div>\r\n        <div class=\"card-body\">\r\n          <!-- Notifikasi -->\r\n          <div v-if=\"notification.message\" :class=\"['notification', notification.type]\">\r\n            {{ notification.message }}\r\n          </div>\r\n\r\n          <form @submit.prevent=\"submitReservasi\">\r\n            <!-- Nama -->\r\n            <div class=\"form-group\">\r\n              <label>Nama</label>\r\n              <input\r\n                type=\"text\"\r\n                class=\"form-control\"\r\n                v-model=\"formData.nama\"\r\n                required\r\n              />\r\n            </div>\r\n\r\n            <!-- Email -->\r\n            <div class=\"form-group\">\r\n              <label>Email</label>\r\n              <input\r\n                type=\"email\"\r\n                class=\"form-control\"\r\n                v-model=\"formData.email\"\r\n                required\r\n              />\r\n            </div>\r\n\r\n            <!-- Nomor Kamar -->\r\n            <div class=\"form-group\">\r\n              <label>Nomor Kamar</label>\r\n              <select\r\n                class=\"form-control\"\r\n                v-model=\"formData.kamar_id\"\r\n                required\r\n              >\r\n                <option value=\"\" disabled>Pilih Nomor Kamar</option>\r\n                <option\r\n                  v-for=\"kamar in kamarTersedia\"\r\n                  :key=\"kamar.id\"\r\n                  :value=\"kamar.id\"\r\n                >\r\n                  {{ kamar.nomor_kamar }}\r\n                </option>\r\n              </select>\r\n            </div>\r\n\r\n            <!-- Periode Penyewaan -->\r\n            <div class=\"form-group\">\r\n              <label>Periode Penyewaan (Bulan)</label>\r\n              <input\r\n                type=\"number\"\r\n                class=\"form-control\"\r\n                v-model=\"formData.periode_penyewaan\"\r\n                @input=\"updateJumlahHarga\"\r\n                required\r\n              />\r\n            </div>\r\n\r\n            <!-- Awal Penyewaan -->\r\n            <div class=\"form-group\">\r\n              <label>Awal Penyewaan</label>\r\n              <input\r\n                type=\"date\"\r\n                class=\"form-control\"\r\n                v-model=\"formData.awal_penyewaan\"\r\n                @input=\"updateAkhirPenyewaan\"\r\n                required\r\n              />\r\n            </div>\r\n\r\n            <!-- Akhir Penyewaan -->\r\n            <div class=\"form-group\">\r\n              <label>Akhir Penyewaan</label>\r\n              <input\r\n                type=\"date\"\r\n                class=\"form-control\"\r\n                v-model=\"formData.akhir_penyewaan\"\r\n                disabled\r\n              />\r\n            </div>\r\n\r\n            <!-- Bukti Pembayaran -->\r\n            <div class=\"form-group\">\r\n              <label>Bukti Pembayaran</label>\r\n              <input\r\n                type=\"file\"\r\n                class=\"form-control\"\r\n                @change=\"handleFileUpload\"\r\n                required\r\n              />\r\n            </div>\r\n\r\n            <!-- Jumlah Harga -->\r\n            <div class=\"form-group\">\r\n              <label>Jumlah Harga</label>\r\n              <input\r\n                type=\"number\"\r\n                class=\"form-control\"\r\n                v-model=\"formData.jumlah_harga\"\r\n                disabled\r\n              />\r\n            </div>\r\n\r\n            <!-- Submit -->\r\n            <button type=\"submit\" class=\"btn btn-primary\">Kirim Reservasi</button>\r\n          </form>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      formData: {\r\n        nama: \"\",\r\n        email: \"\",\r\n        kamar_id: \"\",\r\n        periode_penyewaan: 1,\r\n        awal_penyewaan: \"\",\r\n        akhir_penyewaan: \"\",\r\n        jumlah_harga: 3000000,\r\n        bukti_pembayaran: null,\r\n      },\r\n      kamarTersedia: [],\r\n      notification: { message: \"\", type: \"\" },\r\n    };\r\n  },\r\n  methods: {\r\n    async fetchKamarTersedia() {\r\n      try {\r\n        const response = await axios.get(\"http://127.0.0.1:5000/kamar/tersedia\");\r\n        this.kamarTersedia = response.data.map((kamar) => ({\r\n          id: kamar.id,\r\n          nomor_kamar: kamar.nomor_kamar,\r\n        }));\r\n      } catch (error) {\r\n        this.notification = { message: \"Gagal memuat data kamar.\", type: \"error\" };\r\n      }\r\n    },\r\n    updateJumlahHarga() {\r\n      this.formData.jumlah_harga = this.formData.periode_penyewaan * 3000000;\r\n    },\r\n    updateAkhirPenyewaan() {\r\n      const awal = new Date(this.formData.awal_penyewaan);\r\n      const akhir = new Date(awal);\r\n      akhir.setMonth(awal.getMonth() + parseInt(this.formData.periode_penyewaan, 10));\r\n      this.formData.akhir_penyewaan = akhir.toISOString().split(\"T\")[0];\r\n    },\r\n    handleFileUpload(event) {\r\n      this.formData.bukti_pembayaran = event.target.files[0];\r\n    },\r\n    async submitReservasi() {\r\n      const formDataToSend = new FormData();\r\n      for (const [key, value] of Object.entries(this.formData)) {\r\n        formDataToSend.append(key, value);\r\n      }\r\n\r\n      try {\r\n        const response = await axios.post(\r\n          \"http://127.0.0.1:5000/reservasi\",\r\n          formDataToSend,\r\n          { headers: { \"Content-Type\": \"multipart/form-data\" } }\r\n        );\r\n        this.notification = { message: response.data.message, type: \"success\" };\r\n        this.$router.push(\"/\");\r\n      } catch (error) {\r\n        this.notification = {\r\n          message: error.response.data.error || \"Gagal mengirim data reservasi.\",\r\n          type: \"error\",\r\n        };\r\n      }\r\n    },\r\n  },\r\n  mounted() {\r\n    this.fetchKamarTersedia();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.notification {\r\n  margin-bottom: 20px;\r\n  padding: 10px;\r\n  text-align: center;\r\n}\r\n.notification.success {\r\n  background-color: #28a745;\r\n  color: white;\r\n}\r\n.notification.error {\r\n  background-color: #dc3545;\r\n  color: white;\r\n}\r\n</style>\r\n"],"mappings":";;AA0HA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;QACRC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE,EAAE;QACZC,iBAAiB,EAAE,CAAC;QACpBC,cAAc,EAAE,EAAE;QAClBC,eAAe,EAAE,EAAE;QACnBC,YAAY,EAAE,OAAO;QACrBC,gBAAgB,EAAE;MACpB,CAAC;MACDC,aAAa,EAAE,EAAE;MACjBC,YAAY,EAAE;QAAEC,OAAO,EAAE,EAAE;QAAEC,IAAI,EAAE;MAAG;IACxC,