{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      nama: '',\n      email: '',\n      password: '',\n      no_telp: '',\n      roles: 'penyewa',\n      // Default to \"penyewa\"\n      error: ''\n    };\n  },\n  methods: {\n    async register() {\n      // Clear previous errors\n      this.error = '';\n      try {\n        const response = await axios.post('http://127.0.0.1:5000/register', {\n          nama: this.nama,\n          email: this.email,\n          password: this.password,\n          roles: this.roles,\n          no_telp: this.no_telp\n        });\n        if (response.status === 201) {\n          this.$router.push('/login');\n        }\n      } catch (error) {\n        if (error.response && error.response.data) {\n          this.error = error.response.data.error || 'Registrasi gagal. Silakan coba lagi.';\n        } else {\n          this.error = 'Terjadi kesalahan, silakan coba lagi.';\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","nama","email","password","no_telp","roles","error","methods","register","response","post","status","$router","push"],"sources":["D:\\SEMESTER 5\\Proyek\\Kost\\frontend\\src\\views\\RegisterView.vue"],"sourcesContent":["<template>\r\n    <div class=\"register-page\">\r\n      <h1>Register</h1>\r\n      <form @submit.prevent=\"register\">\r\n        <div class=\"form-group\">\r\n          <input\r\n            type=\"text\"\r\n            v-model=\"nama\"\r\n            placeholder=\"Nama\"\r\n            required\r\n          />\r\n          <input\r\n            type=\"email\"\r\n            v-model=\"email\"\r\n            placeholder=\"Email\"\r\n            required\r\n          />\r\n          <input\r\n            type=\"password\"\r\n            v-model=\"password\"\r\n            placeholder=\"Password\"\r\n            required\r\n            minlength=\"8\"\r\n            maxlength=\"20\"\r\n          />\r\n          <input\r\n            type=\"text\"\r\n            v-model=\"no_telp\"\r\n            placeholder=\"Nomor Telepon\"\r\n            required\r\n          />\r\n          <select v-model=\"roles\" required>\r\n            <option value=\"penyewa\">Penyewa</option>\r\n            <option value=\"pemilik\">Pemilik</option>\r\n            <option value=\"admin\">Admin</option>\r\n          </select>\r\n        </div>\r\n        <button type=\"submit\">Register</button>\r\n      </form>\r\n      <p class=\"login-link\">\r\n        Sudah punya akun? <router-link to=\"/login\">Login</router-link>\r\n      </p>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from 'axios';\r\n  \r\n  export default {\r\n    data() {\r\n      return {\r\n        nama: '',\r\n        email: '',\r\n        password: '',\r\n        no_telp: '',\r\n        roles: 'penyewa', // Default to \"penyewa\"\r\n        error: '',\r\n      };\r\n    },\r\n    methods: {\r\n      async register() {\r\n        // Clear previous errors\r\n        this.error = '';\r\n  \r\n        try {\r\n          const response = await axios.post('http://127.0.0.1:5000/register', {\r\n            nama: this.nama,\r\n            email: this.email,\r\n            password: this.password,\r\n            roles: this.roles,\r\n            no_telp: this.no_telp,\r\n          });\r\n  \r\n          if (response.status === 201) {\r\n            this.$router.push('/login');\r\n          }\r\n        } catch (error) {\r\n          if (error.response && error.response.data) {\r\n            this.error = error.response.data.error || 'Registrasi gagal. Silakan coba lagi.';\r\n          } else {\r\n            this.error = 'Terjadi kesalahan, silakan coba lagi.';\r\n          }\r\n        }\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  /* Styling untuk register page */\r\n  .register-page {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    justify-content: center;\r\n    min-height: 100vh;\r\n    background-color: #f5f5f5;\r\n    padding: 0 20px;\r\n  }\r\n  \r\n  h1 {\r\n    font-size: 2rem;\r\n    font-weight: bold;\r\n    margin-bottom: 20px;\r\n    color: #75276d;\r\n  }\r\n  \r\n  .form-group {\r\n    display: flex;\r\n    flex-direction: column;\r\n    width: 100%;\r\n    max-width: 400px;\r\n  }\r\n  \r\n  input,\r\n  select {\r\n    margin: 10px 0;\r\n    padding: 10px;\r\n    width: 100%;\r\n    border: 1px solid #ccc;\r\n    border-radius: 5px;\r\n  }\r\n  \r\n  button {\r\n    margin: 10px 0;\r\n    padding: 10px;\r\n    width: 100%;\r\n    background-color: #75276d;\r\n    color: white;\r\n    border: none;\r\n    border-radius: 5px;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  button:hover {\r\n    background-color: #5a1c54;\r\n  }\r\n  \r\n  p.login-link {\r\n    margin-top: 20px;\r\n  }\r\n  \r\n  p.login-link router-link {\r\n    color: #75276d;\r\n    text-decoration: none;\r\n  }\r\n  \r\n  .error-message {\r\n    color: red;\r\n    font-size: 0.9rem;\r\n  }\r\n  </style>\r\n  "],"mappings":";AA8CE,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE,EAAE;MACXC,KAAK,EAAE,SAAS;MAAE;MAClBC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,QAAQA,CAAA,EAAG;MACf;MACA,IAAI,CAACF,KAAI,GAAI,EAAE;MAEf,IAAI;QACF,MAAMG,QAAO,GAAI,MAAMV,KAAK,CAACW,IAAI,CAAC,gCAAgC,EAAE;UAClET,IAAI,EAAE,IAAI,CAACA,IAAI;UACfC,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBE,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBD,OAAO,EAAE,IAAI,CAACA;QAChB,CAAC,CAAC;QAEF,IAAIK,QAAQ,CAACE,MAAK,KAAM,GAAG,EAAE;UAC3B,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;QAC7B;MACF,EAAE,OAAOP,KAAK,EAAE;QACd,IAAIA,KAAK,CAACG,QAAO,IAAKH,KAAK,CAACG,QAAQ,CAACT,IAAI,EAAE;UACzC,IAAI,CAACM,KAAI,GAAIA,KAAK,CAACG,QAAQ,CAACT,IAAI,CAACM,KAAI,IAAK,sCAAsC;QAClF,OAAO;UACL,IAAI,CAACA,KAAI,GAAI,uCAAuC;QACtD;MACF;IACF;EACF;AACF,CAAC","ignoreList":[]}