t('http://127.0.0.1:5000/verifikasi');\r\n          this.verifikasiList = response.data;\r\n        } catch (error) {\r\n          console.error('Error fetching verifikasi data:', error);\r\n        }\r\n      },\r\n      async updateVerifikasi(id, status) {\r\n        try {\r\n          await axios.put(`http://127.0.0.1:5000/verifikasi/${id}`, {\r\n            status_verifikasi: status,\r\n          });\r\n          this.fetchVerifikasiList(); // Refresh the list after updating\r\n          alert('Status verifikasi berhasil diperbarui');\r\n        } catch (error) {\r\n          console.error('Error updating verifikasi status:', error);\r\n          alert('Gagal memperbarui status verifikasi');\r\n        }\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .content {\r\n    padding: 20px;\r\n  }\r\n  \r\n  .card-title {\r\n    font-size: 20px;\r\n    font-weight: bold;\r\n  }\r\n  \r\n  .table {\r\n    margin-top: 20px;\r\n  }\r\n  \r\n  .table th {\r\n    text-align: left;\r\n    font-weight: bold;\r\n  }\r\n  \r\n  .text-right {\r\n    text-align: right;\r\n  }\r\n  \r\n  .btn {\r\n    margin-right: 5px;\r\n  }\r\n  \r\n  .btn-sm {\r\n    font-size: 12px;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAmDE,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE;IAClB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,mBAAmB,CAAC,CAAC;EAC5B,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,mBAAmBA,CAAA,EAAG;MAC1B,IAAI;QACF,MAAME,QAAO,GAAI,MAAMN,KAAK,CAACO,GAAG,CAAC,kCAAkC,CAAC;QACpE,IAAI,CAACL,cAAa,GAAII,QAAQ,CAACL,IAAI;MACrC,EAAE,OAAOO,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,iCAAiC,EAAEA,KAAK,CAAC;MACzD;IACF,CAAC;IACD,MAAME,gBAAgBA,CAACC,EAAE,EAAEC,MAAM,EAAE;MACjC,IAAI;QACF,MAAMZ,KAAK,CAACa,GAAG,CAAC,oCAAoCF,EAAE,EAAE,EAAE;UACxDG,iBAAiB,EAAEF;QACrB,CAAC,CAAC;QACF,IAAI,CAACR,mBAAmB,CAAC,CAAC,EAAE;QAC5BW,KAAK,CAAC,uCAAuC,CAAC;MAChD,EAAE,OAAOP,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,mCAAmC,EAAEA,KAAK,CAAC;QACzDO,KAAK,CAAC,qCAAqC,CAAC;MAC9C;IAC