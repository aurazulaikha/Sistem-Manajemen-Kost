{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      users: [],\n      // Data dari API akan disimpan di sini\n      searchQuery: \"\" // Query pencarian\n    };\n  },\n  computed: {\n    filteredUsers() {\n      return this.users.filter(user => {\n        return user.nama.toLowerCase().includes(this.searchQuery.toLowerCase()) || user.email.toLowerCase().includes(this.searchQuery.toLowerCase()) || user.no_telp.includes(this.searchQuery);\n      });\n    }\n  },\n  mounted() {\n    this.fetchUsers();\n  },\n  methods: {\n    async fetchUsers() {\n      try {\n        const response = await axios.get(\"http://127.0.0.1:5000/status-penyewa\");\n        if (response.data.status === \"success\") {\n          this.users = response.data.data; // Assign data dari API ke users\n        } else {\n          console.error(response.data.message);\n        }\n      } catch (error) {\n        console.error(\"Error fetching data:\", error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","users","searchQuery","computed","filteredUsers","filter","user","nama","toLowerCase","includes","email","no_telp","mounted","fetchUsers","methods","response","get","status","console","error","message"],"sources":["D:\\SEMESTER 5\\Proyek\\Web_Kost\\frontend\\src\\views\\StatusPenyewa.vue"],"sourcesContent":["<template>\r\n  <div class=\"content\">\r\n    <div class=\"row\">\r\n      <div class=\"col-md-12\">\r\n        <div class=\"card\">\r\n          <div class=\"card-header d-flex justify-content-between\">\r\n            <h4 class=\"card-title\">Status Penyewa</h4>\r\n            <!-- Search Input -->\r\n            <input\r\n              v-model=\"searchQuery\"\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              placeholder=\"Cari penyewa...\"\r\n              style=\"width: 250px;\"\r\n            />\r\n          </div>\r\n          <div class=\"card-body\">\r\n            <div class=\"table-responsive\">\r\n              <table class=\"table\">\r\n                <thead class=\"text-primary\">\r\n                  <tr>\r\n                    <th>No</th>\r\n                    <th>Nama</th>\r\n                    <th>Email</th>\r\n                    <th>No Telepon</th>\r\n                    <th>Awal Penyewaan</th>\r\n                    <th>Akhir Penyewaan</th>\r\n                  </tr>\r\n                </thead>\r\n                <tbody>\r\n                  <!-- Loop through the users array and display each user with filter -->\r\n                  <tr v-for=\"(user, index) in filteredUsers\" :key=\"index\">\r\n                    <td>{{ index + 1 }}</td>\r\n                    <td>{{ user.nama }}</td>\r\n                    <td>{{ user.email }}</td>\r\n                    <td>{{ user.no_telp }}</td>\r\n                    <td>{{ user.awal_penyewaan }}</td>\r\n                    <td>{{ user.akhir_penyewaan }}</td>\r\n                  </tr>\r\n                  <tr v-if=\"filteredUsers.length === 0\">\r\n                    <td colspan=\"6\">Data tidak ditemukan.</td>\r\n                  </tr>\r\n                </tbody>\r\n              </table>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      users: [], // Data dari API akan disimpan di sini\r\n      searchQuery: \"\" // Query pencarian\r\n    };\r\n  },\r\n  computed: {\r\n    filteredUsers() {\r\n      return this.users.filter(user => {\r\n        return (\r\n          user.nama.toLowerCase().includes(this.searchQuery.toLowerCase()) ||\r\n          user.email.toLowerCase().includes(this.searchQuery.toLowerCase()) ||\r\n          user.no_telp.includes(this.searchQuery)\r\n        );\r\n      });\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchUsers();\r\n  },\r\n  methods: {\r\n    async fetchUsers() {\r\n      try {\r\n        const response = await axios.get(\"http://127.0.0.1:5000/status-penyewa\");\r\n        if (response.data.status === \"success\") {\r\n          this.users = response.data.data; // Assign data dari API ke users\r\n        } else {\r\n          console.error(response.data.message);\r\n        }\r\n      } catch (error) {\r\n        console.error(\"Error fetching data:\", error);\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.table th,\r\n.table td {\r\n  text-align: center;\r\n}\r\n\r\n.table th {\r\n  background-color: #f8f9fa;\r\n}\r\n\r\n.table tbody tr:nth-child(even) {\r\n  background-color: #f2f2f2;\r\n}\r\n</style>\r\n"],"mappings":";;AAqDA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MAAE;MACXC,WAAW,EAAE,EAAC,CAAE;IAClB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,aAAaA,CAAA,EAAG;MACd,OAAO,IAAI,CAACH,KAAK,CAACI,MAAM,CAACC,IAAG,IAAK;QAC/B,OACEA,IAAI,CAACC,IAAI,CAACC,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAACP,WAAW,CAACM,WAAW,CAAC,CAAC,KAC/DF,IAAI,CAACI,KAAK,CAACF,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAACP,WAAW,CAACM,WAAW,CAAC,CAAC,KAChEF,IAAI,CAACK,OAAO,CAACF,QAAQ,CAAC,IAAI,CAACP,WAAW;MAE1C,CAAC,CAAC;IACJ;EACF,CAAC;EACDU,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,UAAU,CAAC,CAAC;EACnB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMhB,KAAK,CAACiB,GAAG,CAAC,sCAAsC,CAAC;QACxE,IAAID,QAAQ,CAACf,IAAI,CAACiB,MAAK,KAAM,SAAS,EAAE;UACtC,IAAI,CAAChB,KAAI,GAAIc,QAAQ,CAACf,IAAI,CAACA,IAAI,EAAE;QACnC,OAAO;UACLkB,OAAO,CAACC,KAAK,CAACJ,QAAQ,CAACf,IAAI,CAACoB,OAAO,CAAC;QACtC;MACF,EAAE,OAAOD,KAAK,EAAE;QACdD,OAAO,CAACC,KAAK,CAAC,sBAAsB,EAAEA,KAAK,CAAC;MAC9C;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}