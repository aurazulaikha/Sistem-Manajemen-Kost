{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      api: 'http://127.0.0.1:5000/verifikasi',\n      verifikasiList: [],\n      searchQuery: '',\n      perPage: 5,\n      currentPage: 1,\n      showDeleteModal: false,\n      verifikasiToDelete: null\n    };\n  },\n  created() {\n    this.fetchVerifikasiList();\n  },\n  computed: {\n    filteredVerifikasi() {\n      return this.verifikasiList.filter(verifikasi => {\n        const searchTerm = this.searchQuery.toLowerCase();\n        return verifikasi.nama && verifikasi.nama.toLowerCase().includes(searchTerm) || verifikasi.email && verifikasi.email.toLowerCase().includes(searchTerm) || verifikasi.no_telp && verifikasi.no_telp.toLowerCase().includes(searchTerm) || verifikasi.nomor_kamar && verifikasi.nomor_kamar.toLowerCase().includes(searchTerm);\n      });\n    },\n    totalPages() {\n      return Math.ceil(this.filteredVerifikasi.length / this.perPage);\n    },\n    paginatedVerifikasi() {\n      const start = (this.currentPage - 1) * this.perPage;\n      const end = start + this.perPage;\n      return this.filteredVerifikasi.slice(start, end);\n    }\n  },\n  methods: {\n    fetchVerifikasiList() {\n      axios.get(this.api).then(response => {\n        this.verifikasiList = response.data;\n      }).catch(error => {\n        console.error('Error fetching verifikasi data:', error);\n      });\n    },\n    updateVerifikasi(id, status) {\n      axios.put(`http://127.0.0.1:5000/verifikasi/${id}`, {\n        status_verifikasi: status\n      }).then(() => {\n        this.fetchVerifikasiList(); // Refresh the list after updating\n        alert('Status verifikasi berhasil diperbarui');\n      }).catch(error => {\n        console.error('Error updating verifikasi status:', error);\n        alert('Gagal memperbarui status verifikasi');\n      });\n    },\n    confirmDelete(verifikasi) {\n      this.verifikasiToDelete = verifikasi;\n      this.showDeleteModal = true;\n    },\n    closeDeleteModal() {\n      this.showDeleteModal = false;\n      this.verifikasiToDelete = null;\n    },\n    deleteVerifikasi() {\n      if (this.verifikasiToDelete && this.verifikasiToDelete.id) {\n        axios.delete(`http://127.0.0.1:5000/verifikasi/${this.verifikasiToDelete.id}`).then(() => {\n          this.fetchVerifikasiList(); // Refresh the list after deletion\n          this.closeDeleteModal();\n        }).catch(error => {\n          console.error('Error deleting verifikasi:', error);\n        });\n      }\n    },\n    previousPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","api","verifikasiList","searchQuery","perPage","currentPage","showDeleteModal","verifikasiToDelete","created","fetchVerifikasiList","computed","filteredVerifikasi","filter","verifikasi","searchTerm","toLowerCase","nama","includes","email","no_telp","nomor_kamar","totalPages","Math","ceil","length","paginatedVerifikasi","start","end","slice","methods","get","then","response","catch","error","console","updateVerifikasi","id","status","put","status_verifikasi","alert","confirmDelete","closeDeleteModal","deleteVerifikasi","delete","previousPage","nextPage"],"sources":["D:\\SEMESTER 5\\Proyek\\Web_Kost\\frontend\\src\\views\\VerifikasiView.vue"],"sourcesContent":["<template>\r\n    <div class=\"content\">\r\n      <div class=\"row\">\r\n        <div class=\"col-md-12\">\r\n          <div class=\"card\">\r\n            <div class=\"card-header d-flex justify-content-between\">\r\n              <h4 class=\"card-title\">Verifikasi Reservasi</h4>\r\n            </div>\r\n            <div class=\"card-body\">\r\n              <div class=\"d-flex justify-content-between mb-3\">\r\n                <input \r\n                  v-model=\"searchQuery\" \r\n                  type=\"text\" \r\n                  class=\"form-control\" \r\n                  placeholder=\"Cari reservasi...\" \r\n                  style=\"width: 250px;\" \r\n                />\r\n              </div>\r\n              <div class=\"table-responsive\">\r\n                <table class=\"table\">\r\n                  <thead class=\"text-primary\">\r\n                    <tr>\r\n                      <th>No</th>\r\n                      <th>Nama</th>\r\n                      <th>Email</th>\r\n                      <th>No. Telepon</th>\r\n                      <th>Bukti Pembayaran</th>\r\n                      <th>Awal Penyewaan</th>\r\n                      <th>Akhir Penyewaan</th>\r\n                      <th>Nomor Kamar</th>\r\n                      <th>Status Verifikasi</th>\r\n                      <th>Aksi</th>\r\n                    </tr>\r\n                  </thead>\r\n                  <tbody>\r\n                    <tr v-for=\"(verifikasi, index) in paginatedVerifikasi\" :key=\"verifikasi.id\">\r\n                      <td>{{ (currentPage - 1) * perPage + (index + 1) }}</td>\r\n                      <td>{{ verifikasi.nama }}</td>\r\n                      <td>{{ verifikasi.email }}</td>\r\n                      <td>{{ verifikasi.no_telp }}</td>\r\n                      <td>\r\n                        <img :src=\"verifikasi.bukti_pembayaran\" alt=\"Bukti Pembayaran\" width=\"100\" />\r\n                      </td>\r\n                      <td>{{ verifikasi.awal_penyewaan }}</td>\r\n                      <td>{{ verifikasi.akhir_penyewaan }}</td>\r\n                      <td>{{ verifikasi.nomor_kamar }}</td>\r\n                      <td>{{ verifikasi.status_verifikasi }}</td>\r\n                      <td class=\"text-right\">\r\n                        <button @click=\"updateVerifikasi(verifikasi.id, 'sudah verifikasi')\" class=\"btn btn-success btn-sm\">Verifikasi</button>\r\n                        <button @click=\"updateVerifikasi(verifikasi.id, 'belum verifikasi')\" class=\"btn btn-warning btn-sm\">Belum Verifikasi</button>\r\n                        <button @click=\"updateVerifikasi(verifikasi.id, 'verifikasi ditotal')\" class=\"btn btn-danger btn-sm\">Verifikasi Ditotal</button>\r\n                      </td>\r\n                    </tr>\r\n                  </tbody>\r\n                </table>\r\n              </div>\r\n  \r\n              <div class=\"pagination-container d-flex justify-content-between mt-3\">\r\n                <span>Halaman {{ currentPage }} dari {{ totalPages }}</span>\r\n                <div class=\"pagination-buttons d-flex justify-content-end\">\r\n                  <button \r\n                    class=\"btn btn-purple btn-sm\" \r\n                    :disabled=\"currentPage === 1\" \r\n                    @click=\"previousPage\"\r\n                  >\r\n                    Previous\r\n                  </button>\r\n                  <button \r\n                    class=\"btn btn-purple btn-sm\" \r\n                    :disabled=\"currentPage === totalPages\" \r\n                    @click=\"nextPage\"\r\n                  >\r\n                    Next\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  \r\n    <div v-if=\"showDeleteModal\" class=\"modal fade show\" tabindex=\"-1\" style=\"display: block;\" aria-modal=\"true\">\r\n      <div class=\"modal-dialog\">\r\n        <div class=\"modal-content\">\r\n          <div class=\"modal-header\">\r\n            <h5 class=\"modal-title\">Konfirmasi Penghapusan</h5>\r\n            <button type=\"button\" class=\"close\" @click=\"closeDeleteModal\">\r\n              <span aria-hidden=\"true\">&times;</span>\r\n            </button>\r\n          </div>\r\n          <div class=\"modal-body\">\r\n            <p>Apakah Anda yakin ingin menghapus reservasi <strong>{{ verifikasiToDelete.nama }}</strong>?</p>\r\n          </div>\r\n          <div class=\"modal-footer\">\r\n            <button type=\"button\" class=\"btn btn-secondary\" @click=\"closeDeleteModal\">Batal</button>\r\n            <button type=\"button\" class=\"btn btn-danger\" @click=\"deleteVerifikasi\">Hapus</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from 'axios';\r\n  \r\n  export default {\r\n    data() {\r\n      return {\r\n        api: 'http://127.0.0.1:5000/verifikasi',\r\n        verifikasiList: [],\r\n        searchQuery: '',\r\n        perPage: 5,\r\n        currentPage: 1,\r\n        showDeleteModal: false,\r\n        verifikasiToDelete: null,\r\n      };\r\n    },\r\n    created() {\r\n      this.fetchVerifikasiList();\r\n    },\r\n    computed: {\r\n      filteredVerifikasi() {\r\n        return this.verifikasiList.filter(verifikasi => {\r\n          const searchTerm = this.searchQuery.toLowerCase();\r\n          return (\r\n            (verifikasi.nama && verifikasi.nama.toLowerCase().includes(searchTerm)) ||\r\n            (verifikasi.email && verifikasi.email.toLowerCase().includes(searchTerm)) ||\r\n            (verifikasi.no_telp && verifikasi.no_telp.toLowerCase().includes(searchTerm)) ||\r\n            (verifikasi.nomor_kamar && verifikasi.nomor_kamar.toLowerCase().includes(searchTerm))\r\n          );\r\n        });\r\n      },\r\n      totalPages() {\r\n        return Math.ceil(this.filteredVerifikasi.length / this.perPage);\r\n      },\r\n      paginatedVerifikasi() {\r\n        const start = (this.currentPage - 1) * this.perPage;\r\n        const end = start + this.perPage;\r\n        return this.filteredVerifikasi.slice(start, end);\r\n      }\r\n    },\r\n    methods: {\r\n      fetchVerifikasiList() {\r\n        axios.get(this.api)\r\n          .then(response => {\r\n            this.verifikasiList = response.data;\r\n          })\r\n          .catch(error => {\r\n            console.error('Error fetching verifikasi data:', error);\r\n          });\r\n      },\r\n      updateVerifikasi(id, status) {\r\n        axios.put(`http://127.0.0.1:5000/verifikasi/${id}`, { status_verifikasi: status })\r\n          .then(() => {\r\n            this.fetchVerifikasiList(); // Refresh the list after updating\r\n            alert('Status verifikasi berhasil diperbarui');\r\n          })\r\n          .catch(error => {\r\n            console.error('Error updating verifikasi status:', error);\r\n            alert('Gagal memperbarui status verifikasi');\r\n          });\r\n      },\r\n      confirmDelete(verifikasi) {\r\n        this.verifikasiToDelete = verifikasi;\r\n        this.showDeleteModal = true;\r\n      },\r\n      closeDeleteModal() {\r\n        this.showDeleteModal = false;\r\n        this.verifikasiToDelete = null;\r\n      },\r\n      deleteVerifikasi() {\r\n        if (this.verifikasiToDelete && this.verifikasiToDelete.id) {\r\n          axios.delete(`http://127.0.0.1:5000/verifikasi/${this.verifikasiToDelete.id}`)\r\n            .then(() => {\r\n              this.fetchVerifikasiList(); // Refresh the list after deletion\r\n              this.closeDeleteModal();\r\n            })\r\n            .catch(error => {\r\n              console.error('Error deleting verifikasi:', error);\r\n            });\r\n        }\r\n      },\r\n      previousPage() {\r\n        if (this.currentPage > 1) {\r\n          this.currentPage--;\r\n        }\r\n      },\r\n      nextPage() {\r\n        if (this.currentPage < this.totalPages) {\r\n          this.currentPage++;\r\n        }\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .table th, .table td {\r\n    text-align: center;\r\n  }\r\n  \r\n  .table th {\r\n    background-color: #f8f9fa;\r\n  }\r\n  \r\n  .btn-sm {\r\n    margin: 0 5px;\r\n  }\r\n  \r\n  .btn-success {\r\n    background-color: #28a745;\r\n  }\r\n  \r\n  .btn-warning {\r\n    background-color: #ffc107;\r\n  }\r\n  \r\n  .btn-danger {\r\n    background-color: #dc3545;\r\n  }\r\n  \r\n  .btn-purple {\r\n    background-color: #75276d;\r\n    color: white;\r\n  }\r\n  \r\n  .pagination-container {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n  }\r\n  \r\n  .pagination-buttons {\r\n    display: flex;\r\n    gap: 10px;\r\n  }\r\n  \r\n  .modal-backdrop {\r\n    z-index: 1040;\r\n  }\r\n  </style>\r\n  "],"mappings":";;AAwGE,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,GAAG,EAAE,kCAAkC;MACvCC,cAAc,EAAE,EAAE;MAClBC,WAAW,EAAE,EAAE;MACfC,OAAO,EAAE,CAAC;MACVC,WAAW,EAAE,CAAC;MACdC,eAAe,EAAE,KAAK;MACtBC,kBAAkB,EAAE;IACtB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,mBAAmB,CAAC,CAAC;EAC5B,CAAC;EACDC,QAAQ,EAAE;IACRC,kBAAkBA,CAAA,EAAG;MACnB,OAAO,IAAI,CAACT,cAAc,CAACU,MAAM,CAACC,UAAS,IAAK;QAC9C,MAAMC,UAAS,GAAI,IAAI,CAACX,WAAW,CAACY,WAAW,CAAC,CAAC;QACjD,OACGF,UAAU,CAACG,IAAG,IAAKH,UAAU,CAACG,IAAI,CAACD,WAAW,CAAC,CAAC,CAACE,QAAQ,CAACH,UAAU,CAAC,IACrED,UAAU,CAACK,KAAI,IAAKL,UAAU,CAACK,KAAK,CAACH,WAAW,CAAC,CAAC,CAACE,QAAQ,CAACH,UAAU,CAAC,IACvED,UAAU,CAACM,OAAM,IAAKN,UAAU,CAACM,OAAO,CAACJ,WAAW,CAAC,CAAC,CAACE,QAAQ,CAACH,UAAU,CAAC,IAC3ED,UAAU,CAACO,WAAU,IAAKP,UAAU,CAACO,WAAW,CAACL,WAAW,CAAC,CAAC,CAACE,QAAQ,CAACH,UAAU,CAAC;MAExF,CAAC,CAAC;IACJ,CAAC;IACDO,UAAUA,CAAA,EAAG;MACX,OAAOC,IAAI,CAACC,IAAI,CAAC,IAAI,CAACZ,kBAAkB,CAACa,MAAK,GAAI,IAAI,CAACpB,OAAO,CAAC;IACjE,CAAC;IACDqB,mBAAmBA,CAAA,EAAG;MACpB,MAAMC,KAAI,GAAI,CAAC,IAAI,CAACrB,WAAU,GAAI,CAAC,IAAI,IAAI,CAACD,OAAO;MACnD,MAAMuB,GAAE,GAAID,KAAI,GAAI,IAAI,CAACtB,OAAO;MAChC,OAAO,IAAI,CAACO,kBAAkB,CAACiB,KAAK,CAACF,KAAK,EAAEC,GAAG,CAAC;IAClD;EACF,CAAC;EACDE,OAAO,EAAE;IACPpB,mBAAmBA,CAAA,EAAG;MACpBV,KAAK,CAAC+B,GAAG,CAAC,IAAI,CAAC7B,GAAG,EACf8B,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAAC9B,cAAa,GAAI8B,QAAQ,CAAChC,IAAI;MACrC,CAAC,EACAiC,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,iCAAiC,EAAEA,KAAK,CAAC;MACzD,CAAC,CAAC;IA