ault {\r\n    data() {\r\n      return {\r\n        email: '',\r\n        password: '',\r\n      };\r\n    },\r\n    methods: {\r\n      async login() {\r\n        try {\r\n          const response = await axios.post('http://127.0.0.1:5000/login', {\r\n            email: this.email,\r\n            password: this.password,\r\n          });\r\n  \r\n          if (response.status === 200) {\r\n            localStorage.setItem('token', response.data.token);\r\n            localStorage.setItem('role', response.data.role);\r\n  \r\n            const role = response.data.role;\r\n            if (role === 'admin' || role === 'pemilik') {\r\n              this.$router.push('/dashboard');\r\n            } else if (role === 'penyewa') {\r\n              this.$router.push('/dashboard-penyewa');\r\n            }\r\n          }\r\n        } catch (error) {\r\n          alert(error.response?.data?.error || 'Login failed');\r\n        }\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  /* Styling for the login page */\r\n  .login-page {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center; /* Center everything */\r\n    justify-content: center;\r\n    min-height: 100vh;\r\n    background-color: #f5f5f5;\r\n    padding: 0 20px;\r\n  }\r\n  \r\n  .login-title {\r\n    font-size: 2rem;\r\n    font-weight: bold; /* Bold text for the title */\r\n    margin-bottom: 20px;\r\n    color: #75276d;\r\n  }\r\n  \r\n  .form-group {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center; /* Center align the inputs and note */\r\n    width: 100%;\r\n    max-width: 400px; /* Optional: sets a maximum width for the form */\r\n  }\r\n  \r\n  input {\r\n    margin: 10px 0;\r\n    padding: 10px;\r\n    width: 100%; /* Full width for inputs */\r\n    border: 1px solid #ccc;\r\n    border-radius: 5px;\r\n  }\r\n  \r\n  button {\r\n    margin: 10px 0;\r\n    padding: 10px;\r\n    width: 100%; /* Full width for the button */\r\n    background-color: #75276d;\r\n    color: white;\r\n    border: none;\r\n    border-radius: 5px;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  button:hover {\r\n    background-color: #5a1c54;\r\n  }\r\n  \r\n  .password-note {\r\n    font-size: 0.8rem; /* Smaller font for the note */\r\n    color: #555;\r\n    margin-top: 5px;\r\n    width: 100%; /* Ensures it spans the same width as the input */\r\n    text-align: left;\r\n  }\r\n  \r\n  /* Styling for the Register link */\r\n  .register-link {\r\n    margin-top: 20px;\r\n    font-size: 0.9rem;\r\n    color: #555;\r\n  }\r\n  \r\n  .register-link a {\r\n    color: #75276d;\r\n    font-weight: bold;\r\n    text-decoration: none;\r\n  }\r\n  \r\n  .register-link a:hover {\r\n    text-decoration: underline;\r\n  }\r\n  </style>\r\n  "],"mappings":";AAkBE,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,KAAKA,CAAA,EAAG;MACZ,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMN,KAAK,CAACO,IAAI,CAAC,6BAA6B,EAAE;UAC/DL,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QAEF,IAAIG,QAAQ,CAACE,MAAK,KAAM,GAAG,EAAE;UAC3BC,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEJ,QAAQ,CAACL,IAAI,CAACU,KAAK,CAAC;UAClDF,YAAY,CAACC,OAAO,CAAC,MAAM,EAAEJ,QAAQ,CAACL,IAAI,CAACW,IAAI,CAAC;UAEhD,MAAMA,IAAG,GAAIN,QAAQ,CAACL,IAAI,CAACW,IAAI;UAC/B,IAAIA,IAAG,KAAM,OAAM,IAAKA,IAAG,KAAM,SAAS,EAAE;YAC1C,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,YAAY,CAAC;UACjC,OAAO,IAAIF,IAAG,KAAM,SAAS,EAAE;YAC7B,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,oBAAoB,CAAC;UACzC;QACF;MACF,EAAE,OAAOC,KAAK,EAAE;QACdC,KAAK,CAACD,KAAK,CAACT,QAAQ,EAAEL,IAAI,EAAEc,KAAI,IAAK,cAAc,CAAC;MACtD;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}