{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      api: 'http://127.0.0.1:5000/list-kamar',\n      rooms: [],\n      searchQuery: '',\n      perPage: 5,\n      currentPage: 1\n    };\n  },\n  created() {\n    this.getRooms();\n  },\n  computed: {\n    filteredRooms() {\n      return this.rooms.filter(room => {\n        const searchTerm = this.searchQuery.toLowerCase();\n        return room.number && room.number.toString().includes(searchTerm) || room.status && room.status.toLowerCase().includes(searchTerm);\n      });\n    },\n    totalPages() {\n      return Math.ceil(this.filteredRooms.length / this.perPage);\n    },\n    paginatedRooms() {\n      const start = (this.currentPage - 1) * this.perPage;\n      const end = start + this.perPage;\n      return this.filteredRooms.slice(start, end);\n    }\n  },\n  methods: {\n    getRooms() {\n      axios.get(this.api).then(response => {\n        if (Array.isArray(response.data)) {\n          this.rooms = response.data;\n        } else {\n          console.error('Data yang diterima tidak sesuai:', response.data);\n        }\n      }).catch(error => {\n        console.error('Terjadi kesalahan:', error);\n      });\n    },\n    goToRoomDetail(roomId) {\n      this.$router.push(`/room-detail/${roomId}`);\n    },\n    previousPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","api","rooms","searchQuery","perPage","currentPage","created","getRooms","computed","filteredRooms","filter","room","searchTerm","toLowerCase","number","toString","includes","status","totalPages","Math","ceil","length","paginatedRooms","start","end","slice","methods","get","then","response","Array","isArray","console","error","catch","goToRoomDetail","roomId","$router","push","previousPage","nextPage"],"sources":["D:\\SEMESTER 5\\Proyek\\Web_Kost\\frontend\\src\\views\\ListKamar.vue"],"sourcesContent":["<template>\r\n    <div class=\"content\">\r\n      <div class=\"row\">\r\n        <div class=\"col-md-12\">\r\n          <div class=\"card\">\r\n            <div class=\"card-header d-flex justify-content-between\">\r\n              <h4 class=\"card-title\">Daftar Kamar Sewa</h4>\r\n            </div>\r\n            <div class=\"card-body\">\r\n              <div class=\"d-flex flex-wrap\">\r\n                <div\r\n                  v-for=\"(room) in paginatedRooms\"\r\n                  :key=\"room.id\"\r\n                  class=\"room-card\"\r\n                  :class=\"room.status\"\r\n                  @click=\"goToRoomDetail(room.id)\"\r\n                  style=\"cursor: pointer;\"\r\n                >\r\n                  <span class=\"room-number\">{{ room.number }}</span>\r\n                </div>\r\n              </div>\r\n  \r\n              <!-- Paginasi -->\r\n              <div class=\"pagination-container d-flex justify-content-between mt-3\">\r\n                <span>Halaman {{ currentPage }} dari {{ totalPages }}</span>\r\n                <div class=\"pagination-buttons d-flex justify-content-end\">\r\n                  <button \r\n                    class=\"btn btn-purple btn-sm\" \r\n                    :disabled=\"currentPage === 1\" \r\n                    @click=\"previousPage\"\r\n                  >\r\n                    Previous\r\n                  </button>\r\n                  <button \r\n                    class=\"btn btn-purple btn-sm\" \r\n                    :disabled=\"currentPage === totalPages\" \r\n                    @click=\"nextPage\"\r\n                  >\r\n                    Next\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from 'axios';\r\n  \r\n  export default {\r\n    data() {\r\n      return {\r\n        api: 'http://127.0.0.1:5000/list-kamar', \r\n        rooms: [], \r\n        searchQuery: '',  \r\n        perPage: 5, \r\n        currentPage: 1,  \r\n      };\r\n    },\r\n    created() {\r\n      this.getRooms();  \r\n    },\r\n    computed: {\r\n      filteredRooms() {\r\n        return this.rooms.filter(room => {\r\n          const searchTerm = this.searchQuery.toLowerCase();\r\n          return (\r\n            (room.number && room.number.toString().includes(searchTerm)) ||\r\n            (room.status && room.status.toLowerCase().includes(searchTerm))\r\n          );\r\n        });\r\n      },\r\n      totalPages() {\r\n        return Math.ceil(this.filteredRooms.length / this.perPage);  \r\n      },\r\n      paginatedRooms() {\r\n        const start = (this.currentPage - 1) * this.perPage;\r\n        const end = start + this.perPage;\r\n        return this.filteredRooms.slice(start, end);  \r\n      }\r\n    },\r\n    methods: {\r\n      getRooms() {\r\n        axios.get(this.api)\r\n          .then(response => {\r\n            if (Array.isArray(response.data)) {\r\n              this.rooms = response.data; \r\n            } else {\r\n              console.error('Data yang diterima tidak sesuai:', response.data);\r\n            }\r\n          })\r\n          .catch(error => {\r\n            console.error('Terjadi kesalahan:', error); \r\n          });\r\n      },\r\n      goToRoomDetail(roomId) {\r\n        this.$router.push(`/room-detail/${roomId}`);\r\n      },\r\n      previousPage() {\r\n        if (this.currentPage > 1) {\r\n          this.currentPage--;  \r\n        }\r\n      },\r\n      nextPage() {\r\n        if (this.currentPage < this.totalPages) {\r\n          this.currentPage++;  \r\n        }\r\n      }\r\n    }\r\n  }\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .room-card {\r\n    width: 100px;\r\n    height: 100px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    margin: 10px;\r\n    border-radius: 10px;\r\n    text-align: center;\r\n    color: white;\r\n    font-size: 18px;\r\n    font-weight: bold;\r\n  }\r\n  \r\n  .room-card.occupied {\r\n    background-color: red;\r\n  }\r\n  \r\n  .room-card.nearlyExpired {\r\n    background-color: yellow;\r\n    color: black;\r\n  }\r\n  \r\n  .room-card.active {\r\n    background-color: green;\r\n  }\r\n  \r\n  .room-card.vacant {\r\n    background-color: gray;\r\n  }\r\n  \r\n  .pagination-container {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n  }\r\n  \r\n  .pagination-buttons {\r\n    display: flex;\r\n    gap: 10px;\r\n  }\r\n  \r\n  .btn-purple {\r\n    background-color: #75276d; \r\n    color: white;\r\n  }\r\n  \r\n  .btn-purple:hover {\r\n    background-color: #5e1d57; \r\n  }\r\n  </style>\r\n  "],"mappings":";;;AAkDE,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,GAAG,EAAE,kCAAkC;MACvCC,KAAK,EAAE,EAAE;MACTC,WAAW,EAAE,EAAE;MACfC,OAAO,EAAE,CAAC;MACVC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,QAAQ,CAAC,CAAC;EACjB,CAAC;EACDC,QAAQ,EAAE;IACRC,aAAaA,CAAA,EAAG;MACd,OAAO,IAAI,CAACP,KAAK,CAACQ,MAAM,CAACC,IAAG,IAAK;QAC/B,MAAMC,UAAS,GAAI,IAAI,CAACT,WAAW,CAACU,WAAW,CAAC,CAAC;QACjD,OACGF,IAAI,CAACG,MAAK,IAAKH,IAAI,CAACG,MAAM,CAACC,QAAQ,CAAC,CAAC,CAACC,QAAQ,CAACJ,UAAU,CAAC,IAC1DD,IAAI,CAACM,MAAK,IAAKN,IAAI,CAACM,MAAM,CAACJ,WAAW,CAAC,CAAC,CAACG,QAAQ,CAACJ,UAAU,CAAC;MAElE,CAAC,CAAC;IACJ,CAAC;IACDM,UAAUA,CAAA,EAAG;MACX,OAAOC,IAAI,CAACC,IAAI,CAAC,IAAI,CAACX,aAAa,CAACY,MAAK,GAAI,IAAI,CAACjB,OAAO,CAAC;IAC5D,CAAC;IACDkB,cAAcA,CAAA,EAAG;MACf,MAAMC,KAAI,GAAI,CAAC,IAAI,CAAClB,WAAU,GAAI,CAAC,IAAI,IAAI,CAACD,OAAO;MACnD,MAAMoB,GAAE,GAAID,KAAI,GAAI,IAAI,CAACnB,OAAO;MAChC,OAAO,IAAI,CAACK,aAAa,CAACgB,KAAK,CAACF,KAAK,EAAEC