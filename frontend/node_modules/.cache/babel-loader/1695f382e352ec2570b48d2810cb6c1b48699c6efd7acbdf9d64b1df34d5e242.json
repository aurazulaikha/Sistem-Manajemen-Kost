{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      kamarList: [],\n      selectedKamar: null,\n      showForm: false,\n      formData: {\n        periode_penyewaan: '',\n        awal_penyewaan: '',\n        akhir_penyewaan: '',\n        bukti_pembayaran: null,\n        jumlah_harga: null\n      }\n    };\n  },\n  created() {\n    this.fetchKamarList();\n  },\n  methods: {\n    async fetchKamarList() {\n      try {\n        const response = await axios.get('http://127.0.0.1:5000/list-kamar');\n        this.kamarList = response.data;\n        console.log(this.kamarList); // Debugging: Pastikan status_kamar sesuai\n      } catch (error) {\n        console.error('Error fetching kamar list:', error);\n      }\n    },\n    getKamarStyle(status) {\n      const statusLower = status.toLowerCase(); // Ubah menjadi huruf kecil untuk memastikan kesesuaian\n\n      if (statusLower === 'tersedia') {\n        return {\n          backgroundColor: '#8BC34A'\n        }; // Hijau lembut\n      } else if (statusLower === 'terisi') {\n        return {\n          backgroundColor: '#B0BEC5'\n        }; // Abu-abu lembut\n      } else if (statusLower === 'masa sewa akan habis') {\n        return {\n          backgroundColor: '#D30000'\n        }; // Merah lembut\n      }\n      return {\n        backgroundColor: '#E0E0E0'\n      }; // default case (grey soft)\n    },\n    getNomorKamarStyle(status) {\n      const statusLower = status.toLowerCase(); // Ubah menjadi huruf kecil untuk memastikan kesesuaian\n\n      if (statusLower === 'masa sewa akan habis') {\n        return {\n          color: 'white'\n        }; // Jika merah, teks menjadi putih\n      }\n      return {\n        color: '#333'\n      }; // default case (warna teks hitam)\n    },\n    showDetail(kamar) {\n      this.selectedKamar = kamar;\n      this.showModal = true;\n    },\n    closeModal() {\n      this.showModal = false;\n      this.selectedKamar = null;\n    },\n    showReservasiForm(kamar) {\n      this.selectedKamar = kamar;\n      this.showForm = true;\n    },\n    closeForm() {\n      this.showForm = false;\n      this.formData = {\n        periode_penyewaan: '',\n        awal_penyewaan: '',\n        akhir_penyewaan: '',\n        bukti_pembayaran: null,\n        jumlah_harga: null\n      };\n    },\n    async submitReservasi() {\n      try {\n        // Kirim data reservasi ke backend\n        const response = await axios.post('http://127.0.0.1:5000/submit-reservasi', {\n          kamar_id: this.selectedKamar.id,\n          ...this.formData\n        });\n        alert('Reservasi berhasil!');\n        this.closeForm();\n      } catch (error) {\n        console.error('Error submitting reservasi:', error);\n        alert('Terjadi kesalahan saat mengirim reservasi.');\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","kamarList","selectedKamar","showForm","formData","periode_penyewaan","awal_penyewaan","akhir_penyewaan","bukti_pembayaran","jumlah_harga","created","fetchKamarList","methods","response","get","console","log","error","getKamarStyle","status","statusLower","toLowerCase","backgroundColor","getNomorKamarStyle","color","showDetail","kamar","showModal","closeModal","showReservasiForm","closeForm","submitReservasi","post","kamar_id","id","alert"],"sources":["D:\\SEMESTER 5\\Proyek\\Web_Kost\\frontend\\src\\views\\ListKamar.vue"],"sourcesContent":["<template>\r\n  <div class=\"kamar-container\">\r\n    <!-- Lantai 1 -->\r\n    <div v-if=\"kamarList.length > 0\">\r\n      <h2>Lantai 1</h2>\r\n      <div class=\"kamar-row\">\r\n        <div v-for=\"kamar in kamarList.filter(k => k.id <= 10)\" :key=\"kamar.id\" \r\n             class=\"kamar-card\"\r\n             :style=\"getKamarStyle(kamar.status_kamar)\"\r\n             @click=\"kamar.status_kamar === 'tersedia' ? showReservasiForm(kamar) : null\">\r\n          <div class=\"kamar-item\">\r\n            <div class=\"nomor-kamar\" :style=\"getNomorKamarStyle(kamar.status_kamar)\">{{ kamar.nomor_kamar }}</div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Lantai 2 -->\r\n    <div v-if=\"kamarList.length > 0\">\r\n      <h2>Lantai 2</h2>\r\n      <div class=\"kamar-row\">\r\n        <div v-for=\"kamar in kamarList.filter(k => k.id > 10 && k.id <= 23)\" :key=\"kamar.id\" \r\n             class=\"kamar-card\"\r\n             :style=\"getKamarStyle(kamar.status_kamar)\"\r\n             @click=\"kamar.status_kamar === 'tersedia' ? showReservasiForm(kamar) : null\">\r\n          <div class=\"kamar-item\">\r\n            <div class=\"nomor-kamar\" :style=\"getNomorKamarStyle(kamar.status_kamar)\">{{ kamar.nomor_kamar }}</div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Form Reservasi -->\r\n    <div v-if=\"showForm\" class=\"form-reservasi\">\r\n      <h3>Form Reservasi Kamar</h3>\r\n      <form @submit.prevent=\"submitReservasi\">\r\n        <div>\r\n          <label>Periode Penyewaan:</label>\r\n          <input v-model=\"formData.periode_penyewaan\" type=\"text\" required />\r\n        </div>\r\n        <div>\r\n          <label>Awal Penyewaan:</label>\r\n          <input v-model=\"formData.awal_penyewaan\" type=\"date\" required />\r\n        </div>\r\n        <div>\r\n          <label>Akhir Penyewaan:</label>\r\n          <input v-model=\"formData.akhir_penyewaan\" type=\"date\" required />\r\n        </div>\r\n        <div>\r\n          <label>Bukti Pembayaran:</label>\r\n          <input v-model=\"formData.bukti_pembayaran\" type=\"file\" required />\r\n        </div>\r\n        <div>\r\n          <label>Jumlah Harga:</label>\r\n          <input v-model=\"formData.jumlah_harga\" type=\"number\" required />\r\n        </div>\r\n        <button type=\"submit\">Kirim Reservasi</button>\r\n        <button type=\"button\" @click=\"closeForm\">Tutup</button>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      kamarList: [],\r\n      selectedKamar: null,\r\n      showForm: false,\r\n      formData: {\r\n        periode_penyewaan: '',\r\n        awal_penyewaan: '',\r\n        akhir_penyewaan: '',\r\n        bukti_pembayaran: null,\r\n        jumlah_harga: null\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    this.fetchKamarList();\r\n  },\r\n  methods: {\r\n    async fetchKamarList() {\r\n      try {\r\n        const response = await axios.get('http://127.0.0.1:5000/list-kamar');\r\n        this.kamarList = response.data;\r\n        console.log(this.kamarList);  // Debugging: Pastikan status_kamar sesuai\r\n      } catch (error) {\r\n        console.error('Error fetching kamar list:', error);\r\n      }\r\n    },\r\n    getKamarStyle(status) {\r\n      const statusLower = status.toLowerCase();  // Ubah menjadi huruf kecil untuk memastikan kesesuaian\r\n\r\n      if (statusLower === 'tersedia') {\r\n        return { backgroundColor: '#8BC34A' };  // Hijau lembut\r\n      } else if (statusLower === 'terisi') {\r\n        return { backgroundColor: '#B0BEC5' };  // Abu-abu lembut\r\n      } else if (statusLower === 'masa sewa akan habis') {\r\n        return { backgroundColor: '#D30000' };  // Merah lembut\r\n      }\r\n      return { backgroundColor: '#E0E0E0' };  // default case (grey soft)\r\n    },\r\n    getNomorKamarStyle(status) {\r\n      const statusLower = status.toLowerCase();  // Ubah menjadi huruf kecil untuk memastikan kesesuaian\r\n\r\n      if (statusLower === 'masa sewa akan habis') {\r\n        return { color: 'white' };  // Jika merah, teks menjadi putih\r\n      }\r\n      return { color: '#333' };  // default case (warna teks hitam)\r\n    },\r\n    showDetail(kamar) {\r\n      this.selectedKamar = kamar;\r\n      this.showModal = true;\r\n    },\r\n    closeModal() {\r\n      this.showModal = false;\r\n      this.selectedKamar = null;\r\n    },\r\n    showReservasiForm(kamar) {\r\n      this.selectedKamar = kamar;\r\n      this.showForm = true;\r\n    },\r\n    closeForm() {\r\n      this.showForm = false;\r\n      this.formData = {\r\n        periode_penyewaan: '',\r\n        awal_penyewaan: '',\r\n        akhir_penyewaan: '',\r\n        bukti_pembayaran: null,\r\n        jumlah_harga: null\r\n      };\r\n    },\r\n    async submitReservasi() {\r\n      try {\r\n        // Kirim data reservasi ke backend\r\n        const response = await axios.post('http://127.0.0.1:5000/submit-reservasi', {\r\n          kamar_id: this.selectedKamar.id,\r\n          ...this.formData\r\n        });\r\n        alert('Reservasi berhasil!');\r\n        this.closeForm();\r\n      } catch (error) {\r\n        console.error('Error submitting reservasi:', error);\r\n        alert('Terjadi kesalahan saat mengirim reservasi.');\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.kamar-container {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 40px;\r\n  padding: 20px;\r\n  margin-top: 50px;\r\n}\r\n\r\n.form-reservasi {\r\n  background-color: #fff;\r\n  padding: 20px;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n  width: 300px;\r\n  margin-top: 20px;\r\n}\r\n\r\n.form-reservasi form {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 10px;\r\n}\r\n\r\n.kamar-row {\r\n  display: flex;\r\n  gap: 20px;\r\n  flex-wrap: wrap;\r\n}\r\n\r\n.kamar-card {\r\n  width: 140px;\r\n  height: 140px;\r\n  background-color: white;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  cursor: pointer;\r\n  transition: transform 0.3s;\r\n}\r\n\r\n.kamar-card:hover {\r\n  transform: scale(1.05);\r\n  box-shadow: 0 6px 12px rgba(0, 0, 0, 0.2);\r\n}\r\n\r\n.kamar-item {\r\n  text-align: center;\r\n}\r\n\r\n.nomor-kamar {\r\n  color: #333;\r\n  font-size: 18px;\r\n  font-weight: bold;\r\n}\r\n\r\nbutton {\r\n  padding: 10px 20px;\r\n  background-color: #c20ab0;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #9b0a7f;\r\n}\r\n</style>\r\n"],"mappings":"AAgEA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,aAAa,EAAE,IAAI;MACnBC,QAAQ,EAAE,KAAK;MACfC,QAAQ,EAAE;QACRC,iBAAiB,EAAE,EAAE;QACrBC,cAAc,EAAE,EAAE;QAClBC,eAAe,EAAE,EAAE;QACnBC,gBAAgB,EAAE,IAAI;QACtBC,YAAY,EAAE;MAChB;IACF,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,cAAc,CAAC,CAAC;EACvB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,cAAcA,CAAA,EAAG;MACrB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMd,KAAK,CAACe,GAAG,CAAC,kCAAkC,CAAC;QACpE,IAAI,CAACb,SAAQ,GAAIY,QAAQ,CAACb,IAAI;QAC9Be,OAAO,CAACC,GAAG,CAAC,IAAI,CAACf,SAAS,CAAC,EAAG;MAChC,EAAE,OAAOgB,KAAK,EAAE;QACdF,OAAO,CAACE,KAAK,CAAC,4BAA4B,EAAEA,KAAK,CAAC;MACpD;IACF,CAAC;IACDC,aAAaA,CAACC,MAAM,EAAE;MACpB,MAAMC,WAAU,GAAID,MAAM,CAACE,WAAW,CAAC,CAAC,EAAG;;MAE3C,IAAID,WAAU,KAAM,UAAU,EAAE;QAC9B,OAAO;UAAEE,eAAe,EAAE;QAAU,CAAC,EAAG;MAC1C,OAAO,IAAIF,WAAU,KAAM,QAAQ,EAAE;QACnC,OAAO;UAAEE,eAAe,EAAE;QAAU,CAAC,EAAG;MAC1C,OAAO,IAAIF,WAAU,KAAM,sBAAsB,EAAE;QACjD,OAAO;UAAEE,eAAe,EAAE;QAAU,CAAC,EAAG;MAC1C;MACA,OAAO;QAAEA,eAAe,EAAE;MAAU,CAAC,EAAG;IAC1C,CAAC;IACDC,kBAAkBA,CAACJ,MAAM,EAAE;MACzB,MAAMC,WAAU,GAAID,MAAM,CAACE,WAAW,CAAC,CAAC,EAAG;;MAE3C,IAAID,WAAU,KAAM,sBAAsB,EAAE;QAC1C,OAAO;UAAEI,KAAK,EAAE;QAAQ,CAAC,EAAG;MAC9B;MACA,OAAO;QAAEA,KAAK,EAAE;MAAO,CAAC,EAAG;IAC7B,CAAC;IACDC,UAAUA,CAACC,KAAK,EAAE;MAChB,IAAI,CAACxB,aAAY,GAAIwB,KAAK;MAC1B,IAAI,CAACC,SAAQ,GAAI,IAAI;IACvB,CAAC;IACDC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACD,SAAQ,GAAI,KAAK;MACtB,IAAI,CAACzB,aAAY,GAAI,IAAI;IAC3B,CAAC;IACD2B,iBAAiBA,CAACH,KAAK,EAAE;MACvB,IAAI,CAACxB,aAAY,GAAIwB,KAAK;MAC1B,IAAI,CAACvB,QAAO,GAAI,IAAI;IACtB,CAAC;IACD2B,SAASA,CAAA,EAAG;MACV,IAAI,CAAC3B,QAAO,GAAI,KAAK;MACrB,IAAI,CAACC,QAAO,GAAI;QACdC,iBAAiB,EAAE,EAAE;QACrBC,cAAc,EAAE,EAAE;QAClBC,eAAe,EAAE,EAAE;QACnBC,gBAAgB,EAAE,IAAI;QACtBC,YAAY,EAAE;MAChB,CAAC;IACH,CAAC;IACD,MAA