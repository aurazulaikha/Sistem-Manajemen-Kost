d,\r\n          });\r\n  \r\n          if (response.status === 200) {\r\n            localStorage.setItem('token', response.data.token);\r\n            localStorage.setItem('role', response.data.role);\r\n  \r\n            const role = response.data.role;\r\n            if (role === 'admin' || role === 'pemilik') {\r\n              this.$router.push('/dashboard');\r\n            } else if (role === 'penyewa') {\r\n              this.$router.push('/dashboard-penyewa');\r\n            }\r\n          }\r\n        } catch (error) {\r\n          alert(error.response?.data?.error || 'Login failed');\r\n        }\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n    \r\n    <style scoped>\r\n    /* Styling untuk login page */\r\n    input {\r\n      margin: 10px;\r\n      padding: 10px;\r\n    }\r\n    button {\r\n      margin: 10px;\r\n      padding: 10px;\r\n      background-color: #75276d;\r\n      color: white;\r\n      border: none;\r\n      cursor: pointer;\r\n    }\r\n    </style>\r\n    "],"mappings":";AAYE,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,KAAKA,CAAA,EAAG;MACZ,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMN,KAAK,CAACO,IAAI,CAAC,6BAA6B,EAAE;UAC/DL,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QAEF,IAAIG,QAAQ,CAACE,MAAK,KAAM,GAAG,EAAE;UAC3BC,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEJ,QAAQ,CAACL,IAAI,CAACU,KAAK,CAAC;UAClDF,YAAY,CAACC,OAAO,CAAC,MAAM,EAAEJ,QAAQ,CAACL,IAAI,CAACW,IAAI,CAAC;UAEhD,MAAMA,IAAG,GAAIN,QAAQ,CAACL,IAAI,CAACW,IAAI;UAC/B,IAAIA,IAAG,KAAM,OAAM,IAAKA,IAAG,KAAM,SAAS,EAAE;YAC1C,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,YAAY,CAAC;UACjC,OAAO,IAAIF,IAAG,KAAM,SAAS,EAAE;YAC7B,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,oBAAoB,CAAC;UACzC;QACF;MACF,EAAE,OAAOC,KAAK,EAAE;QACdC,KAAK,CAACD,KAAK,CAACT,QAAQ,EAAEL,IAAI,EAAEc,KAAI,IAAK,cAAc,CAAC;MACtD;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}