{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nexport default {\n  data() {\n    return {\n      users: [],\n      // Data penyewa dari API\n      searchQuery: '',\n      // Input pencarian\n      perPage: 5,\n      // Jumlah data per halaman\n      currentPage: 1 // Halaman aktif\n    };\n  },\n  computed: {\n    // Filter data berdasarkan input pencarian\n    filteredUsers() {\n      return this.users.filter(user => {\n        const searchTerm = this.searchQuery.toLowerCase();\n        return user.nama && user.nama.toLowerCase().includes(searchTerm) || user.email && user.email.toLowerCase().includes(searchTerm) || user.no_telp && user.no_telp.includes(this.searchQuery) || user.status && user.status.toLowerCase().includes(searchTerm);\n      });\n    },\n    // Total halaman berdasarkan hasil filter\n    totalPages() {\n      return Math.ceil(this.filteredUsers.length / this.perPage);\n    },\n    // Data yang akan ditampilkan pada halaman tertentu\n    paginatedUsers() {\n      const start = (this.currentPage - 1) * this.perPage;\n      const end = start + this.perPage;\n      return this.filteredUsers.slice(start, end);\n    }\n  },\n  methods: {\n    // Mengambil data dari API\n    fetchStatusPenyewa() {\n      fetch('http://127.0.0.1:5000/status-penyewa').then(response => {\n        if (!response.ok) {\n          throw new Error('Gagal mengambil data dari API');\n        }\n        return response.json();\n      }).then(data => {\n        this.users = data; // Isi data ke `users`\n      }).catch(error => {\n        console.error('Error:', error);\n      });\n    },\n    // Navigasi ke halaman sebelumnya\n    previousPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    },\n    // Navigasi ke halaman berikutnya\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    }\n  },\n  mounted() {\n    this.fetchStatusPenyewa(); // Ambil data saat komponen dimuat\n  }\n};","map":{"version":3,"names":["data","users","searchQuery","perPage","currentPage","computed","filteredUsers","filter","user","searchTerm","toLowerCase","nama","includes","email","no_telp","status","totalPages","Math","ceil","length","paginatedUsers","start","end","slice","methods","fetchStatusPenyewa","fetch","then","response","ok","Error","json","catch","error","console","previousPage","nextPage","mounted"],"sources":["D:\\SEMESTER 5\\Proyek\\Web_Kost\\frontend\\src\\views\\StatusPenyewa.vue"],"sourcesContent":["<template>\r\n  <div class=\"content\">\r\n    <div class=\"row\">\r\n      <div class=\"col-md-12\">\r\n        <div class=\"card\">\r\n          <div class=\"card-header d-flex justify-content-between\">\r\n            <h4 class=\"card-title\">Status Penyewa</h4>\r\n            <!-- Search Input -->\r\n            <input \r\n              v-model=\"searchQuery\" \r\n              type=\"text\" \r\n              class=\"form-control\" \r\n              placeholder=\"Cari penyewa...\" \r\n              style=\"width: 250px;\" \r\n            />\r\n          </div>\r\n          <div class=\"card-body\">\r\n            <div class=\"table-responsive\">\r\n              <table class=\"table\">\r\n                <thead class=\"text-primary\">\r\n                  <tr>\r\n                    <th>No</th>\r\n                    <th>Nama</th>\r\n                    <th>Email</th>\r\n                    <th>Phone</th>\r\n                    <th>Awal Penyewaan</th>\r\n                    <th>Akhir Penyewaan</th>\r\n                    <th>Status</th>\r\n                  </tr>\r\n                </thead>\r\n                <tbody>\r\n                  <tr v-for=\"(user, index) in paginatedUsers\" :key=\"index\">\r\n                    <td>{{ (currentPage - 1) * perPage + (index + 1) }}</td>\r\n                    <td>{{ user.nama }}</td>\r\n                    <td>{{ user.email }}</td>\r\n                    <td>{{ user.no_telp }}</td>\r\n                    <td>{{ user.awal_penyewaan }}</td>\r\n                    <td>{{ user.akhir_penyewaan }}</td>\r\n                    <td>{{ user.status }}</td>\r\n                  </tr>\r\n                  <!-- Tampilkan pesan jika data tidak ditemukan -->\r\n                  <tr v-if=\"filteredUsers.length === 0\">\r\n                    <td colspan=\"7\">Tidak ada data yang sesuai.</td>\r\n                  </tr>\r\n                </tbody>\r\n              </table>\r\n            </div>\r\n            \r\n            <!-- Pagination Controls -->\r\n            <div class=\"pagination-container d-flex justify-content-between mt-3\">\r\n              <span>Halaman {{ currentPage }} dari {{ totalPages }}</span>\r\n              <div class=\"pagination-buttons d-flex justify-content-end\">\r\n                <button \r\n                  class=\"btn btn-purple btn-sm\" \r\n                  :disabled=\"currentPage === 1\" \r\n                  @click=\"previousPage\"\r\n                >\r\n                  Previous\r\n                </button>\r\n                <button \r\n                  class=\"btn btn-purple btn-sm\" \r\n                  :disabled=\"currentPage === totalPages\" \r\n                  @click=\"nextPage\"\r\n                >\r\n                  Next\r\n                </button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      users: [], // Data penyewa dari API\r\n      searchQuery: '', // Input pencarian\r\n      perPage: 5, // Jumlah data per halaman\r\n      currentPage: 1, // Halaman aktif\r\n    };\r\n  },\r\n  computed: {\r\n    // Filter data berdasarkan input pencarian\r\n    filteredUsers() {\r\n      return this.users.filter(user => {\r\n        const searchTerm = this.searchQuery.toLowerCase();\r\n        return (\r\n          (user.nama && user.nama.toLowerCase().includes(searchTerm)) ||\r\n          (user.email && user.email.toLowerCase().includes(searchTerm)) ||\r\n          (user.no_telp && user.no_telp.includes(this.searchQuery)) ||\r\n          (user.status && user.status.toLowerCase().includes(searchTerm))\r\n        );\r\n      });\r\n    },\r\n    // Total halaman berdasarkan hasil filter\r\n    totalPages() {\r\n      return Math.ceil(this.filteredUsers.length / this.perPage);\r\n    },\r\n    // Data yang akan ditampilkan pada halaman tertentu\r\n    paginatedUsers() {\r\n      const start = (this.currentPage - 1) * this.perPage;\r\n      const end = start + this.perPage;\r\n      return this.filteredUsers.slice(start, end);\r\n    }\r\n  },\r\n  methods: {\r\n    // Mengambil data dari API\r\n    fetchStatusPenyewa() {\r\n      fetch('http://127.0.0.1:5000/status-penyewa')\r\n        .then(response => {\r\n          if (!response.ok) {\r\n            throw new Error('Gagal mengambil data dari API');\r\n          }\r\n          return response.json();\r\n        })\r\n        .then(data => {\r\n          this.users = data; // Isi data ke `users`\r\n        })\r\n        .catch(error => {\r\n          console.error('Error:', error);\r\n        });\r\n    },\r\n    // Navigasi ke halaman sebelumnya\r\n    previousPage() {\r\n      if (this.currentPage > 1) {\r\n        this.currentPage--;\r\n      }\r\n    },\r\n    // Navigasi ke halaman berikutnya\r\n    nextPage() {\r\n      if (this.currentPage < this.totalPages) {\r\n        this.currentPage++;\r\n      }\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchStatusPenyewa(); // Ambil data saat komponen dimuat\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.table th, .table td {\r\n  text-align: center;\r\n}\r\n\r\n.table th {\r\n  background-color: #f8f9fa;\r\n}\r\n\r\n.btn-sm {\r\n  margin: 0 5px;\r\n}\r\n\r\n.btn-purple {\r\n  background-color: #75276d;\r\n  color: white;\r\n}\r\n\r\n.btn-purple:hover {\r\n  background-color: #5e1d57;\r\n}\r\n\r\n.pagination-container {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.pagination-buttons {\r\n  display: flex;\r\n  gap: 10px;\r\n}\r\n</style>\r\n"],"mappings":";;AA4EA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MAAE;MACXC,WAAW,EAAE,EAAE;MAAE;MACjBC,OAAO,EAAE,CAAC;MAAE;MACZC,WAAW,EAAE,CAAC,CAAE;IAClB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR;IACAC,aAAaA,CAAA,EAAG;MACd,OAAO,IAAI,CAACL,KAAK,CAACM,MAAM,CAACC,IAAG,IAAK;QAC/B,MAAMC,UAAS,GAAI,IAAI,CAACP,WAAW,CAACQ,WAAW,CAAC,CAAC;QACjD,OACGF,IAAI,CAACG,IAAG,IAAKH,IAAI,CAACG,IAAI,CAACD,WAAW,CAAC,CAAC,CAACE,QAAQ,CAACH,UAAU,CAAC,IACzDD,IAAI,CAACK,KAAI,IAAKL,IAAI,CAACK,KAAK,CAACH,WAAW,CAAC,CAAC,CAACE,QAAQ,CAACH,UAAU,CAAC,IAC3DD,IAAI,CAACM,OAAM,IAAKN,IAAI,CAACM,OAAO,CAACF,QAAQ,CAAC,IAAI,CAACV,WAAW,CAAC,IACvDM,IAAI,CAACO,MAAK,IAAKP,IAAI,CAACO,MAAM,CAACL,WAAW,CAAC,CAAC,CAACE,QAAQ,CAACH,UAAU,CAAC;MAElE,CAAC,CAAC;IACJ,CAAC;IACD;IACAO,UAAUA,CAAA,EAAG;MACX,OAAOC,IAAI,CAACC,IAAI,CAAC,IAAI,CAACZ,aAAa,CAACa,MAAK,GAAI,IAAI,CAAChB,OAAO,CAAC;IAC5D,CAAC;IACD;IACAiB,cAAcA,CAAA,EAAG;MACf,MAAMC,KAAI,GAAI,CAAC,IAAI,CAACjB,WAAU,GAAI,CAAC,IAAI,IAAI,CAACD,OAAO;MACnD,MAAMmB,GAAE,GAAID,KAAI,GAAI,IAAI,CAAClB,OAAO;MAChC,OAAO,IAAI,CAACG,aAAa,CAACiB,KAAK,CAACF,KAAK,EAAEC,GAAG,CAAC;IAC7C;EACF,CAAC;EACDE,OAAO,EAAE;IACP;IACAC,kBAAkBA,CAAA,EAAG;MACnBC,KAAK,CAAC,sCAAsC,EACzCC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACA,QAAQ,CAACC,EAAE,EAAE;UAChB,MAAM,IAAIC,KAAK,CAAC,+BAA+B,CAAC;QAClD;QACA,OAAOF,QAAQ,CAACG,IAAI,CAAC,CAAC;MACxB,CAAC,EACAJ,IAAI,CAAC3B,IAAG,IAAK;QACZ,IAAI,CAACC,KAAI,GAAID,IAAI,EAAE;MACrB,CAAC,EACAgC,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,QAAQ,EAAEA,KAAK,CAAC;MAChC,CAAC,CAAC;IACN,CAAC;IACD;IACAE,YAAYA,CAAA,EAAG;MACb,IAAI,IAAI,CAAC/B,WAAU,GAAI,CAAC,EAAE;QACxB,IAAI,CAACA,WAAW,EAAE;MACpB;IACF,CAAC;IACD;IACAgC,QAAQA,CAAA,EAAG;MACT,IAAI,IAAI,CAAChC,WAAU,GAAI,IAAI,CAACY,UAAU,EAAE;QACtC,IAAI,CAACZ,WAAW,EAAE;MACpB;IACF;EACF,CAAC;EACDiC,OAAOA,CAAA,EAAG;IA