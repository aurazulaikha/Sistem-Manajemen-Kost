a();\n      Object.entries(this.formData).forEach(([key, value]) => {\n        formDataToSend.append(key, value);\n      });\n      try {\n        const response = await axios.post('http://127.0.0.1:5000/reservasi', formDataToSend, {\n          headers: {\n            'Content-Type': 'multipart/form-data'\n          }\n        });\n        this.notification = {\n          message: response.data.message,\n          type: 'success'\n        };\n        this.$router.push('/');\n      } catch (error) {\n        console.error('Error submitting reservasi:', error);\n        this.notification = {\n          message: 'Terjadi kesalahan saat mengirim reservasi.',\n          type: 'error'\n        };\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","formData","kamar_id","$route","params","kamarId","nama","email","nomor_telepon","periode_penyewaan","awal_penyewaan","akhir_penyewaan","bukti_pembayaran","jumlah_harga","catatan_tambahan","errors","notification","message","type","methods","handleFileUpload","event","file","target","files","submitReservasi","Object","keys","length","formDataToSend","FormData","entries","forEach","key","value","append","response","post","headers","$router","push","error","console"],"sources":["D:\\SEMESTER 5\\Proyek\\Web_Kost\\frontend\\src\\views\\ReservasiKamar.vue"],"sourcesContent":["<template>\r\n  <div class=\"content\">\r\n    <div class=\"col-md-8\">\r\n      <div class=\"card card-user\">\r\n        <div class=\"card-header\">\r\n          <h5 class=\"card-title\">Form Reservasi Kamar</h5>\r\n        </div>\r\n        <div class=\"card-body\">\r\n          <!-- Notifikasi -->\r\n          <div v-if=\"notification.message\" :class=\"['notification', notification.type]\">\r\n            {{ notification.message }}\r\n          </div>\r\n\r\n          <form @submit.prevent=\"submitReservasi\">\r\n            <!-- Field Nama -->\r\n            <div class=\"row\">\r\n              <div class=\"col-md-12\">\r\n                <div class=\"form-group\">\r\n                  <label>Nama</label>\r\n                  <input \r\n                    type=\"text\" \r\n                    class=\"form-control\" \r\n                    placeholder=\"Nama\"\r\n                    v-model=\"formData.nama\"\r\n                    :class=\"{'is-invalid': errors.nama}\"\r\n                    required\r\n                  />\r\n                  <div v-if=\"errors.nama\" class=\"invalid-feedback\">{{ errors.nama }}</div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n\r\n            <!-- Field Nomor Telepon -->\r\n            <div class=\"row\">\r\n              <div class=\"col-md-12\">\r\n                <div class=\"form-group\">\r\n                  <label>Nomor Telepon</label>\r\n                  <input \r\n                    type=\"text\" \r\n                    class=\"form-control\" \r\n                    placeholder=\"Nomor Telepon\"\r\n                    v-model=\"formData.nomor_telepon\"\r\n                    :class=\"{'is-invalid': errors.nomor_telepon}\"\r\n                    required\r\n                  />\r\n                  <div v-if=\"errors.nomor_telepon\" class=\"invalid-feedback\">{{ errors.nomor_telepon }}</div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n\r\n            <!-- Field Email -->\r\n            <div class=\"row\">\r\n              <div class=\"col-md-12\">\r\n                <div class=\"form-group\">\r\n                  <label>Email</label>\r\n                  <input \r\n                    type=\"email\" \r\n                    class=\"form-control\" \r\n                    placeholder=\"Email\"\r\n                    v-model=\"formData.email\"\r\n                    :class=\"{'is-invalid': errors.email}\"\r\n                    required\r\n                  />\r\n                  <div v-if=\"errors.email\" class=\"invalid-feedback\">{{ errors.email }}</div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n\r\n            <!-- Field Periode Penyewaan -->\r\n            <div class=\"row\">\r\n              <div class=\"col-md-12\">\r\n                <div class=\"form-group\">\r\n                  <label>Periode Penyewaan</label>\r\n                  <input \r\n                    type=\"text\" \r\n                    class=\"form-control\" \r\n                    placeholder=\"Periode Penyewaan\"\r\n                    v-model=\"formData.periode_penyewaan\"\r\n                    :class=\"{'is-invalid': errors.periode_penyewaan}\"\r\n                    required\r\n                  />\r\n                  <div v-if=\"errors.periode_penyewaan\" class=\"invalid-feedback\">{{ errors.periode_penyewaan }}</div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n\r\n            <!-- Field Awal Penyewaan -->\r\n            <div class=\"row\">\r\n              <div class=\"col-md-12\">\r\n                <div class=\"form-group\">\r\n                  <label>Awal Penyewaan</label>\r\n                  <input \r\n                    type=\"date\" \r\n                    class=\"form-control\" \r\n                    v-model=\"formData.awal_penyewaan\"\r\n                    :class=\"{'is-invalid': errors.awal_penyewaan}\"\r\n                    required\r\n                  />\r\n                  <div v-if=\"errors.awal_penyewaan\" class=\"invalid-feedback\">{{ errors.awal_penyewaan }}</div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n\r\n            <!-- Field Akhir Penyewaan -->\r\n            <div class=\"row\">\r\n              <div class=\"col-md-12\">\r\n                <div class=\"form-group\">\r\n                  <label>Akhir Penyewaan</label>\r\n                  <input \r\n                    type=\"date\" \r\n                    class=\"form-control\" \r\n                    v-model=\"formData.akhir_penyewaan\"\r\n                    :class=\"{'is-invalid': errors.akhir_penyewaan}\"\r\n                    required\r\n                  />\r\n                  <div v-if=\"errors.akhir_penyewaan\" class=\"invalid-feedback\">{{ errors.akhir_penyewaan }}</div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n\r\n            <!-- Field Catatan Tambahan -->\r\n            <div class=\"row\">\r\n              <div class=\"col-md-12\">\r\n                <div class=\"form-group\">\r\n                  <label>Catatan Tambahan</label>\r\n                  <textarea\r\n                    class=\"form-control\"\r\n                    placeholder=\"Catatan Tambahan (Opsional)\"\r\n                    v-model=\"formData.catatan_tambahan\"\r\n                    :class=\"{'is-invalid': errors.catatan_tambahan}\"\r\n                  ></textarea>\r\n                  <div v-if=\"errors.catatan_tambahan\" class=\"invalid-feedback\">{{ errors.catatan_tambahan }}</div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n\r\n            <!-- Field Bukti Pembayaran -->\r\n            <div class=\"row\">\r\n              <div class=\"col-md-12\">\r\n                <div class=\"form-group\">\r\n                  <label>Bukti Pembayaran</label>\r\n                  <input \r\n                    type=\"file\" \r\n                    class=\"form-control\"\r\n                    @change=\"handleFileUpload\"\r\n                    :class=\"{'is-invalid': errors.bukti_pembayaran}\"\r\n                    required\r\n                  />\r\n                  <div v-if=\"errors.bukti_pembayaran\" class=\"invalid-feedback\">{{ errors.bukti_pembayaran }}</div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n\r\n            <!-- Field Jumlah Harga -->\r\n            <div class=\"row\">\r\n              <div class=\"col-md-12\">\r\n                <div class=\"form-group\">\r\n                  <label>Jumlah Harga</label>\r\n                  <input \r\n                    type=\"number\" \r\n                    class=\"form-control\" \r\n                    placeholder=\"Jumlah Harga\" \r\n                    v-model=\"formData.jumlah_harga\"\r\n                    :class=\"{'is-invalid': errors.jumlah_harga}\"\r\n                    required\r\n                  />\r\n                  <div v-if=\"errors.jumlah_harga\" class=\"invalid-feedback\">{{ errors.jumlah_harga }}</div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n\r\n            <!-- Tombol Submit -->\r\n            <div class=\"row\">\r\n              <div class=\"update ml-auto mr-auto\">\r\n                <button type=\"submit\" class=\"btn btn-update-profile btn-round\">Kirim Reservasi</button>\r\n              </div>\r\n            </div>\r\n          </form>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      formData: {\r\n        kamar_id: this.$route.params.kamarId,\r\n        nama: '',\r\n        email: '',\r\n        nomor_telepon: '',\r\n        periode_penyewaan: '',\r\n        awal_penyewaan: '',\r\n        akhir_penyewaan: '',\r\n        bukti_pembayaran: null,\r\n        jumlah_harga: null,\r\n        catatan_tambahan: '',\r\n      },\r\n      errors: {},\r\n      notification: {\r\n        message: '',\r\n        type: '',\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    handleFileUpload(event) {\r\n      const file = event.target.files[0];\r\n      if (file) {\r\n        this.formData.bukti_pembayaran = file;\r\n      } else {\r\n        this.errors.bukti_pembayaran = 'File bukti pembayaran harus dipilih';\r\n      }\r\n    },\r\n    async submitReservasi() {\r\n      this.errors = {};\r\n      this.notification = {};\r\n\r\n      // Validasi data\r\n      if (!this.formData.nama) this.errors.nama = 'Nama tidak boleh kosong';\r\n      if (!this.formData.nomor_telepon) this.errors.nomor_telepon = 'Nomor telepon tidak boleh kosong';\r\n      if (!this.formData.email) this.errors.email = 'Email tidak boleh kosong';\r\n      if (!this.formData.periode_penyewaan) this.errors.periode_penyewaan = 'Periode penyewaan tidak boleh kosong';\r\n      if (!this.formData.awal_penyewaan) this.errors.awal_penyewaan = 'Tanggal awal tidak boleh kosong';\r\n      if (!this.formData.akhir_penyewaan) this.errors.akhir_penyewaan = 'Tanggal akhir tidak boleh kosong';\r\n      if (!this.formData.jumlah_harga) this.errors.jumlah_harga = 'Jumlah harga tidak boleh kosong';\r\n      if (!this.formData.bukti_pembayaran) this.errors.bukti_pembayaran = 'Bukti pembayaran tidak boleh kosong';\r\n\r\n      if (Object.keys(this.errors).length > 0) return;\r\n\r\n      // Mengirim data\r\n      const formDataToSend = new FormData();\r\n      Object.entries(this.formData).forEach(([key, value]) => {\r\n        formDataToSend.append(key, value);\r\n      });\r\n\r\n      try {\r\n        const response = await axios.post('http://127.0.0.1:5000/reservasi', formDataToSend, {\r\n          headers: { 'Content-Type': 'multipart/form-data' },\r\n        });\r\n        this.notification = { message: response.data.message, type: 'success' };\r\n        this.$router.push('/');\r\n      } catch (error) {\r\n        console.error('Error submitting reservasi:', error);\r\n        this.notification = { message: 'Terjadi kesalahan saat mengirim reservasi.', type: 'error' };\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.notification {\r\n  margin-bottom: 20px;\r\n  padding: 10px;\r\n  border-radius: 5px;\r\n  font-weight: bold;\r\n}\r\n.notification.success {\r\n  background-color: #d4edda;\r\n  color: #155724;\r\n}\r\n.notification.error {\r\n  background-color: #f8d7da;\r\n  color: #721c24;\r\n}\r\n.is-invalid {\r\n  border-color: #dc3545;\r\n}\r\n.invalid-feedback {\r\n  color: #dc3545;\r\n}\r\n</style>\r\n"],"mappings":";;;AAyLA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MA