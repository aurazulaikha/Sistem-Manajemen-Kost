{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  data() {\n    return {\n      formData: {\n        nama: \"\",\n        email: \"\",\n        kamar_id: \"\",\n        periode_penyewaan: 2,\n        awal_penyewaan: \"\",\n        akhir_penyewaan: \"\",\n        jumlah_harga: 3000000,\n        bukti_pembayaran: null\n      },\n      kamarTersedia: [],\n      notification: {\n        message: \"\",\n        type: \"\"\n      }\n    };\n  },\n  methods: {\n    // Mengambil data kamar yang tersedia dari backend\n    async fetchKamarTersedia() {\n      try {\n        const response = await axios.get(\"http://127.0.0.1:5000/available_rooms\");\n        this.kamarTersedia = response.data.rooms; // Pastikan API mengembalikan data yang tepat\n      } catch (error) {\n        this.notification = {\n          message: \"Gagal memuat data kamar.\",\n          type: \"error\"\n        };\n      }\n    },\n    // Mengupdate harga berdasarkan periode penyewaan\n    updateJumlahHarga() {\n      this.formData.jumlah_harga = this.formData.periode_penyewaan * 3000000;\n    },\n    // Menghitung akhir penyewaan berdasarkan awal penyewaan\n    updateAkhirPenyewaan() {\n      const awal = new Date(this.formData.awal_penyewaan);\n      const akhir = new Date(awal);\n      akhir.setMonth(awal.getMonth() + parseInt(this.formData.periode_penyewaan, 10));\n      this.formData.akhir_penyewaan = akhir.toISOString().split(\"T\")[0];\n    },\n    // Menangani file upload bukti pembayaran\n    handleFileUpload(event) {\n      this.formData.bukti_pembayaran = event.target.files[0];\n    },\n    // Mengirim data reservasi ke backend\n    async submitReservasi() {\n      // Membuat FormData untuk mengirim file\n      const formDataToSend = new FormData();\n      formDataToSend.append('nama', this.formData.nama);\n      formDataToSend.append('email', this.formData.email);\n      formDataToSend.append('kamar_id', this.formData.kamar_id);\n      formDataToSend.append('periode_penyewaan', this.formData.periode_penyewaan);\n      formDataToSend.append('awal_penyewaan', this.formData.awal_penyewaan);\n      formDataToSend.append('bukti_pembayaran', this.formData.bukti_pembayaran);\n      try {\n        // Melakukan POST request untuk mengirim data reservasi\n        const response = await axios.post(\"http://127.0.0.1:5000/reservasi\", formDataToSend, {\n          headers: {\n            \"Content-Type\": \"multipart/form-data\"\n          }\n        });\n        this.notification = {\n          message: response.data.message,\n          type: \"success\"\n        };\n      } catch (error) {\n        // Menampilkan pesan error jika terjadi kesalahan\n        this.notification = {\n          message: error.response.data.error,\n          type: \"error\"\n        };\n      }\n    }\n  },\n  mounted() {\n    // Mengambil data kamar yang tersedia saat komponen dimuat\n    this.fetchKamarTersedia();\n  }\n};","map":{"version":3,"names":["axios","data","formData","nama","email","kamar_id","periode_penyewaan","awal_penyewaan","akhir_penyewaan","jumlah_harga","bukti_pembayaran","kamarTersedia","notification","message","type","methods","fetchKamarTersedia","response","get","rooms","error","updateJumlahHarga","updateAkhirPenyewaan","awal","Date","akhir","setMonth","getMonth","parseInt","toISOString","split","handleFileUpload","event","target","files","submitReservasi","formDataToSend","FormData","append","post","headers","mounted"],"sources":["D:\\SEMESTER 5\\Proyek\\Web_Kost\\frontend\\src\\views\\ReservasiKamar.vue"],"sourcesContent":["<template>\r\n  <div class=\"content\">\r\n    <div class=\"col-md-8\">\r\n      <div class=\"card card-user\">\r\n        <div class=\"card-header\">\r\n          <h5 class=\"card-title\">Form Reservasi Kamar</h5>\r\n        </div>\r\n        <div class=\"card-body\">\r\n          <!-- Form Reservasi -->\r\n          <form @submit.prevent=\"submitReservasi\">\r\n            <div class=\"form-group\">\r\n              <label>Nama</label>\r\n              <input type=\"text\" class=\"form-control\" v-model=\"formData.nama\" required />\r\n            </div>\r\n\r\n            <div class=\"form-group\">\r\n              <label>Email</label>\r\n              <input type=\"email\" class=\"form-control\" v-model=\"formData.email\" required />\r\n            </div>\r\n\r\n            <div class=\"form-group\">\r\n              <label>Nomor Kamar</label>\r\n              <select class=\"form-control\" v-model=\"formData.kamar_id\" required>\r\n                <option value=\"\" disabled>Pilih Nomor Kamar</option>\r\n                <option v-for=\"kamar in kamarTersedia\" :key=\"kamar.id\" :value=\"kamar.id\">\r\n                  {{ kamar.nomor_kamar }}\r\n                </option>\r\n              </select>\r\n            </div>\r\n\r\n            <div class=\"form-group\">\r\n              <label>Periode Penyewaan (Bulan)</label>\r\n              <input type=\"number\" class=\"form-control\" v-model=\"formData.periode_penyewaan\" @input=\"updateJumlahHarga\" required min=\"1\" />\r\n            </div>\r\n\r\n            <div class=\"form-group\">\r\n              <label>Awal Penyewaan</label>\r\n              <input type=\"date\" class=\"form-control\" v-model=\"formData.awal_penyewaan\" @input=\"updateAkhirPenyewaan\" required />\r\n            </div>\r\n\r\n            <!-- Hapus field akhir_penyewaan -->\r\n            <!-- <div class=\"form-group\">\r\n              <label>Akhir Penyewaan</label>\r\n              <input type=\"date\" class=\"form-control\" v-model=\"formData.akhir_penyewaan\" disabled />\r\n            </div> -->\r\n\r\n            <div class=\"form-group\">\r\n              <label>Bukti Pembayaran</label>\r\n              <input type=\"file\" class=\"form-control\" @change=\"handleFileUpload\" required />\r\n              <!-- Tampilkan keterangan file yang diupload atau pesan jika belum diupload -->\r\n              <p v-if=\"formData.bukti_pembayaran\">\r\n                File yang diupload: {{ formData.bukti_pembayaran.name }}\r\n              </p>\r\n              <p v-else>Belum ada file yang diupload.</p>\r\n            </div>\r\n\r\n            <div class=\"form-group\">\r\n              <label>Jumlah Harga</label>\r\n              <input type=\"number\" class=\"form-control\" v-model=\"formData.jumlah_harga\" disabled />\r\n            </div>\r\n\r\n            <!-- Button Kirim Reservasi -->\r\n            <button type=\"submit\" class=\"btn\" :style=\"{ backgroundColor: '#75276d', color: '#fff' }\">Kirim Reservasi</button>\r\n          </form>\r\n          \r\n          <!-- Notifikasi -->\r\n          <div v-if=\"notification.message\" :class=\"['notification', notification.type]\">\r\n            {{ notification.message }}\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      formData: {\r\n        nama: \"\",\r\n        email: \"\",\r\n        kamar_id: \"\",\r\n        periode_penyewaan: 2,\r\n        awal_penyewaan: \"\",\r\n        akhir_penyewaan: \"\",\r\n        jumlah_harga: 3000000,\r\n        bukti_pembayaran: null,\r\n      },\r\n      kamarTersedia: [],\r\n      notification: { message: \"\", type: \"\" },\r\n    };\r\n  },\r\n  methods: {\r\n    // Mengambil data kamar yang tersedia dari backend\r\n    async fetchKamarTersedia() {\r\n      try {\r\n        const response = await axios.get(\"http://127.0.0.1:5000/available_rooms\");\r\n        this.kamarTersedia = response.data.rooms;  // Pastikan API mengembalikan data yang tepat\r\n      } catch (error) {\r\n        this.notification = { message: \"Gagal memuat data kamar.\", type: \"error\" };\r\n      }\r\n    },\r\n\r\n    // Mengupdate harga berdasarkan periode penyewaan\r\n    updateJumlahHarga() {\r\n      this.formData.jumlah_harga = this.formData.periode_penyewaan * 3000000;\r\n    },\r\n\r\n    // Menghitung akhir penyewaan berdasarkan awal penyewaan\r\n    updateAkhirPenyewaan() {\r\n      const awal = new Date(this.formData.awal_penyewaan);\r\n      const akhir = new Date(awal);\r\n      akhir.setMonth(awal.getMonth() + parseInt(this.formData.periode_penyewaan, 10));\r\n      this.formData.akhir_penyewaan = akhir.toISOString().split(\"T\")[0];\r\n    },\r\n\r\n    // Menangani file upload bukti pembayaran\r\n    handleFileUpload(event) {\r\n      this.formData.bukti_pembayaran = event.target.files[0];\r\n    },\r\n\r\n    // Mengirim data reservasi ke backend\r\n    async submitReservasi() {\r\n      // Membuat FormData untuk mengirim file\r\n      const formDataToSend = new FormData();\r\n      formDataToSend.append('nama', this.formData.nama);\r\n      formDataToSend.append('email', this.formData.email);\r\n      formDataToSend.append('kamar_id', this.formData.kamar_id);\r\n      formDataToSend.append('periode_penyewaan', this.formData.periode_penyewaan);\r\n      formDataToSend.append('awal_penyewaan', this.formData.awal_penyewaan);\r\n      formDataToSend.append('bukti_pembayaran', this.formData.bukti_pembayaran);\r\n\r\n      try {\r\n        // Melakukan POST request untuk mengirim data reservasi\r\n        const response = await axios.post(\r\n          \"http://127.0.0.1:5000/reservasi\",\r\n          formDataToSend,\r\n          { headers: { \"Content-Type\": \"multipart/form-data\" } }\r\n        );\r\n        this.notification = { message: response.data.message, type: \"success\" };\r\n      } catch (error) {\r\n        // Menampilkan pesan error jika terjadi kesalahan\r\n        this.notification = { message: error.response.data.error, type: \"error\" };\r\n      }\r\n    },\r\n  },\r\n  mounted() {\r\n    // Mengambil data kamar yang tersedia saat komponen dimuat\r\n    this.fetchKamarTersedia();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Styling untuk notifikasi sukses dan error */\r\n.notification {\r\n  margin-top: 15px;\r\n  padding: 10px;\r\n  border-radius: 5px;\r\n  text-align: center;\r\n}\r\n\r\n.notification.success {\r\n  background-color: #d4edda;\r\n  color: #155724;\r\n}\r\n\r\n.notification.error {\r\n  background-color: #f8d7da;\r\n  color: #721c24;\r\n}\r\n\r\n/* Styling untuk form input */\r\n.form-group {\r\n  margin-bottom: 15px;\r\n}\r\n\r\ninput[type=\"file\"] {\r\n  padding: 10px;\r\n}\r\n\r\n/* Button styling */\r\nbutton {\r\n  margin-top: 15px;\r\n  width: 100%;\r\n}\r\n</style>\r\n"],"mappings":"AA4EA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;QACRC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE,EAAE;QACZC,iBAAiB,EAAE,CAAC;QACpBC,cAAc,EAAE,EAAE;QAClBC,eAAe,EAAE,EAAE;QACnBC,YAAY,EAAE,OAAO;QACrBC,gBAAgB,EAAE;MACpB,CAAC;MACDC,aAAa,EAAE,EAAE;MACjBC,YAAY,EAAE;QAAEC,OAAO,EAAE,EAAE;QAAEC,IAAI,EAAE;MAAG;IACxC,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP;IACA,MAAMC,kBAAkBA,CAAA,EAAG;MACzB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMjB,KAAK,CAACkB,GAAG,CAAC,uCAAuC,CAAC;QACzE,IAAI,CAACP,aAAY,GAAIM,QAAQ,CAAChB,IAAI,CAACkB,KAAK,EAAG;MAC7C,EAAE,OAAOC,KAAK,EAAE;QACd,IAAI,CAACR,YAAW,GAAI;UAAEC,OAAO,EAAE,0BAA0B;UAAEC,IAAI,EAAE;QAAQ,CAAC;MAC5E;IACF,CAAC;IAED;IACAO,iBAAiBA,CAAA,EAAG;MAClB,IAAI,CAACnB,QAAQ,CAACO,YAAW,GAAI,IAAI,CAACP,QAAQ,CAACI,iBAAgB,GAAI,OAAO;IACxE,CAAC;IAED;IACAgB,oBAAoBA,CAAA,EAAG;MACrB,MAAMC,IAAG,GAAI,IAAIC,IAAI,CAAC,IAAI,CAACtB,QAAQ,CAACK,cAAc,CAAC;MACnD,MAAMkB,KAAI,GAAI,IAAID,IAAI,CAACD,IAAI,CAAC;MAC5BE,KAAK,CAACC,QAAQ,CAACH,IAAI,CAACI,QAAQ,CAAC,IAAIC,QAAQ,CAAC,IAAI,CAAC1B,QAAQ,CAACI,iBAAiB,EAAE,EAAE,CAAC,CAAC;MAC/E,IAAI,CAACJ,QAAQ,CAACM,eAAc,GAAIiB,KAAK,CAACI,WAAW,CAAC,CAAC,CAACC,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC,CAAC;IACnE,CAAC;IAED;IACAC,gBAAgBA,CAACC,KAAK,EAAE;MACtB,IAAI,CAAC9B,QAAQ,CAACQ,gBAAe,GAAIsB,KAAK,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;IACxD,CAAC;IAED;IACA,MAAMC,eAAeA,CAAA,EAAG;MACtB;MACA,MAAMC,cAAa,GAAI,IAAIC,QAAQ,CAAC,CAAC;MACrCD,cAAc,CAACE,MAAM,CAAC,MAAM,EAAE,IAAI,CAACpC,QAAQ,CAACC,IAAI,CAAC;MACjDiC,cAAc,CAACE,MAAM,CAAC,OAAO,EAAE,