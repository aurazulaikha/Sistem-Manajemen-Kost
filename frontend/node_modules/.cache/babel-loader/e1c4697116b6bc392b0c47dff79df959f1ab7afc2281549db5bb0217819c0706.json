{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      api: 'http://127.0.0.1:5000/list-user',\n      users: [],\n      currentUsers: {\n        id: '',\n        nama: '',\n        email: '',\n        roles: '',\n        no_telp: ''\n      }\n    };\n  },\n  mounted() {\n    console.log('DOM mounted');\n  },\n  created() {\n    console.log('DOM created');\n    this.getUsers();\n  },\n  methods: {\n    getUsers() {\n      axios.get(this.api).then(response => {\n        console.log(response.data.users);\n        this.users = response.data.users;\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","data","api","users","currentUsers","id","nama","email","roles","no_telp","mounted","console","log","created","getUsers","methods","get","then","response","catch","error"],"sources":["D:\\SEMESTER 5\\Proyek\\Web_Kost\\frontend\\src\\views\\ListUser.vue"],"sourcesContent":["<template>\r\n  <div class=\"content\">\r\n    <div class=\"row\">\r\n      <div class=\"col-md-12\">\r\n        <div class=\"card\">\r\n          <div class=\"card-header d-flex justify-content-between\">\r\n            <h4 class=\"card-title\">Daftar User</h4>\r\n            <!-- Search Input -->\r\n            <input \r\n              v-model=\"searchQuery\" \r\n              type=\"text\" \r\n              class=\"form-control\" \r\n              placeholder=\"Cari user...\" \r\n              style=\"width: 250px;\" \r\n            />\r\n          </div>\r\n          <div class=\"card-body\">\r\n            <div class=\"table-responsive\">\r\n              <table class=\"table\">\r\n                <thead class=\"text-primary\">\r\n                  <tr>\r\n                    <th>No</th>\r\n                    <th>ID</th>\r\n                    <th>Nama</th>\r\n                    <th>Email</th>\r\n                    <th>Role</th>\r\n                    <th>Phone</th>\r\n                    <th>Action</th>\r\n                  </tr>\r\n                </thead>\r\n                <tbody>\r\n                  <!-- Loop through the users array and display each user with filter -->\r\n                  <tr v-for=\"users in users\">\r\n                    <td>{{ index + 1 }}</td>\r\n                    <td>{{ users.id }}</td>\r\n                    <td>{{ users.nama }}</td>\r\n                    <td>{{ users.email }}</td>\r\n                    <td>{{ users.roles }}</td>\r\n                    <td>{{ users.no_telp }}</td>\r\n                    <td class=\"text-right\">\r\n                      <router-link :to=\"'/edit-user/' + user.id\">\r\n                        <button class=\"btn btn-warning btn-sm\">Edit</button>\r\n                      </router-link>\r\n                      <button class=\"btn btn-danger btn-sm\" @click=\"confirmDelete(user.id)\">Hapus</button>\r\n                    </td>\r\n                  </tr>\r\n                </tbody>\r\n              </table>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n  \r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      api: 'http://127.0.0.1:5000/list-user',\r\n      users: [],\r\n      currentUsers: {\r\n        id: '',\r\n        nama: '',\r\n        email: '',\r\n        roles: '',\r\n        no_telp: ''\r\n      }\r\n    };\r\n  },\r\n  mounted(){\r\n    console.log('DOM mounted')\r\n  },\r\n\r\n  created(){\r\n    console.log('DOM created')\r\n    this.getUsers()\r\n  },\r\n\r\n  methods: {\r\n    getUsers() {\r\n      axios.get(this.api)\r\n        .then(response => {\r\n          console.log(response.data.users);\r\n          this.users = response.data.users;\r\n        })\r\n        .catch(error => {\r\n          console.error(error);\r\n        });\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* Styling for the table and buttons */\r\n.table th, .table td {\r\n  text-align: center;\r\n}\r\n\r\n.table th {\r\n  background-color: #f8f9fa;\r\n}\r\n\r\n.btn-sm {\r\n  margin: 0 5px;\r\n}\r\n\r\n.btn-info {\r\n  background-color: #17a2b8;\r\n}\r\n\r\n.btn-success {\r\n  background-color: #28a745;\r\n}\r\n\r\n.btn-warning {\r\n  background-color: #ffc107;\r\n}\r\n\r\n.btn-danger {\r\n  background-color: #dc3545;\r\n}\r\n</style>\r\n"],"mappings":"AAyDA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,GAAG,EAAE,iCAAiC;MACtCC,KAAK,EAAE,EAAE;MACTC,YAAY,EAAE;QACZC,EAAE,EAAE,EAAE;QACNC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE,EAAE;QACTC,KAAK,EAAE,EAAE;QACTC,OAAO,EAAE;MACX;IACF,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAE;IACPC,OAAO,CAACC,GAAG,CAAC,aAAa;EAC3B,CAAC;EAEDC,OAAOA,CAAA,EAAE;IACPF,OAAO,CAACC,GAAG,CAAC,aAAa;IACzB,IAAI,CAACE,QAAQ,CAAC;EAChB,CAAC;EAEDC,OAAO,EAAE;IACPD,QAAQA,CAAA,EAAG;MACTd,KAAK,CAACgB,GAAG,CAAC,IAAI,CAACd,GAAG,EACfe,IAAI,CAACC,QAAO,IAAK;QAChBP,OAAO,CAACC,GAAG,CAACM,QAAQ,CAACjB,IAAI,CAACE,KAAK,CAAC;QAChC,IAAI,CAACA,KAAI,GAAIe,QAAQ,CAACjB,IAAI,CAACE,KAAK;MAClC,CAAC,EACAgB,KAAK,CAACC,KAAI,IAAK;QACdT,OAAO,CAACS,KAAK,CAACA,KAAK,CAAC;MACtB,CAAC,CAAC;IACN;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}