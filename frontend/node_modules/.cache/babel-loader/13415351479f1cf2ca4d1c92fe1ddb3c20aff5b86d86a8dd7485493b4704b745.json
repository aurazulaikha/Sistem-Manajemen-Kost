{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      api: 'http://127.0.0.1:5000/list-penyewa',\n      // Endpoint API yang benar\n      penyewa: [],\n      searchQuery: '' // Untuk fitur pencarian\n    };\n  },\n  created() {\n    this.getPenyewa(); // Mengambil data user saat created\n  },\n  computed: {\n    // Filter users berdasarkan pencarian yang ada di searchQuery\n    filteredPenyewa() {\n      return this.penyewa.filter(penyewa => {\n        const searchTerm = this.searchQuery.toLowerCase();\n\n        // Menambahkan pengecekan apakah properti `user.id`, `user.nama`, dll ada dan bukan undefined\n        return penyewa.nama && penyewa.nama.toLowerCase().includes(searchTerm) || penyewa.email && penyewa.email.toLowerCase().includes(searchTerm) || penyewa.no_telp && penyewa.no_telp.toLowerCase().includes(searchTerm);\n      });\n    }\n  },\n  methods: {\n    getPenyewa() {\n      axios.get(this.api).then(response => {\n        // Pastikan data yang diterima memiliki format yang benar\n        if (Array.isArray(response.data)) {\n          this.penyewa = response.data; // Simpan data ke dalam state 'users'\n        } else {\n          console.error('Data yang diterima tidak sesuai:', response.data);\n        }\n      }).catch(error => {\n        console.error('Terjadi kesalahan:', error); // Cek error jika ada masalah dalam request\n      });\n    },\n    confirmDelete(penyewaId) {\n      console.log(`Penyewa dengan ID ${penyewaId} akan dihapus`); // Logika hapus bisa ditambahkan di sini\n    }\n  }\n};","map":{"version":3,"names":["axios","data","api","penyewa","searchQuery","created","getPenyewa","computed","filteredPenyewa","filter","searchTerm","toLowerCase","nama","includes","email","no_telp","methods","get","then","response","Array","isArray","console","error","catch","confirmDelete","penyewaId","log"],"sources":["D:\\SEMESTER 5\\Proyek\\Web_Kost\\frontend\\src\\views\\ListPenyewa.vue"],"sourcesContent":["<template>\r\n  <div class=\"content\">\r\n    <div class=\"row\">\r\n      <div class=\"col-md-12\">\r\n        <div class=\"card\">\r\n          <div class=\"card-header d-flex justify-content-between\">\r\n            <h4 class=\"card-title\">Daftar Penyewa</h4>\r\n          </div>\r\n          <div class=\"card-body\">\r\n            <div class=\"d-flex justify-content-between mb-3\">\r\n              <!-- Button Tambah User dengan warna ungu tua -->\r\n              <router-link to=\"/tambah-user\">\r\n                <button class=\"btn btn-purple btn-sm\">Tambah Data</button>\r\n              </router-link>\r\n              <!-- Search Input -->\r\n              <input \r\n                v-model=\"searchQuery\" \r\n                type=\"text\" \r\n                class=\"form-control\" \r\n                placeholder=\"Cari user...\" \r\n                style=\"width: 250px;\" \r\n              />\r\n            </div>\r\n            <div class=\"table-responsive\">\r\n              <table class=\"table\">\r\n                <thead class=\"text-primary\">\r\n                  <tr>\r\n                    <th>No</th>\r\n                    <th>Nama</th>\r\n                    <th>Email</th>\r\n                    <th>Phone</th>\r\n                    <th>Action</th>\r\n                  </tr>\r\n                </thead>\r\n                <tbody>\r\n                  <!-- Loop through the filteredUsers array to display users with filter applied -->\r\n                  <tr v-for=\"(penyewa, index) in filteredUsers\" :key=\"penyewa.id\">\r\n                    <td>{{ index + 1 }}</td>\r\n                    <td>{{ penyewa.nama }}</td>\r\n                    <td>{{ penyewa.email }}</td>\r\n                    <td>{{ penyewa.no_telp }}</td>\r\n                    <td class=\"text-right\">\r\n                      <router-link :to=\"'/edit-penyewa/' + penyewa.id\">\r\n                        <button class=\"btn btn-warning btn-sm\">Edit</button>\r\n                      </router-link>\r\n                      <button class=\"btn btn-danger btn-sm\" @click=\"confirmDelete(user.id)\">Hapus</button>\r\n                    </td>\r\n                  </tr>\r\n                </tbody>\r\n              </table>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      api: 'http://127.0.0.1:5000/list-penyewa',  // Endpoint API yang benar\r\n      penyewa: [],\r\n      searchQuery: '' // Untuk fitur pencarian\r\n    };\r\n  },\r\n  created() {\r\n    this.getPenyewa();  // Mengambil data user saat created\r\n  },\r\n  computed: {\r\n    // Filter users berdasarkan pencarian yang ada di searchQuery\r\n    filteredPenyewa() {\r\n      return this.penyewa.filter(penyewa => {\r\n        const searchTerm = this.searchQuery.toLowerCase();\r\n\r\n        // Menambahkan pengecekan apakah properti `user.id`, `user.nama`, dll ada dan bukan undefined\r\n        return (\r\n          (penyewa.nama && penyewa.nama.toLowerCase().includes(searchTerm)) ||\r\n          (penyewa.email && penyewa.email.toLowerCase().includes(searchTerm)) ||\r\n          (penyewa.no_telp && penyewa.no_telp.toLowerCase().includes(searchTerm))\r\n        );\r\n      });\r\n    }\r\n  },\r\n  methods: {\r\n    getPenyewa() {\r\n      axios.get(this.api)\r\n        .then(response => {\r\n          // Pastikan data yang diterima memiliki format yang benar\r\n          if (Array.isArray(response.data)) {\r\n            this.penyewa = response.data;  // Simpan data ke dalam state 'users'\r\n          } else {\r\n            console.error('Data yang diterima tidak sesuai:', response.data);\r\n          }\r\n        })\r\n        .catch(error => {\r\n          console.error('Terjadi kesalahan:', error);  // Cek error jika ada masalah dalam request\r\n        });\r\n    },\r\n    confirmDelete(penyewaId) {\r\n      console.log(`Penyewa dengan ID ${penyewaId} akan dihapus`);  // Logika hapus bisa ditambahkan di sini\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* Styling untuk tabel dan tombol */\r\n.table th, .table td {\r\n  text-align: center;\r\n}\r\n\r\n.table th {\r\n  background-color: #f8f9fa;\r\n}\r\n\r\n.btn-sm {\r\n  margin: 0 5px;\r\n}\r\n\r\n.btn-warning {\r\n  background-color: #ffc107;\r\n}\r\n\r\n.btn-danger {\r\n  background-color: #dc3545;\r\n}\r\n\r\n.btn-purple {\r\n  background-color: #75276d; /* Warna ungu tua */\r\n  color: white;\r\n}\r\n\r\n.btn-purple:hover {\r\n  background-color: #5e1d57; /* Efek hover lebih gelap */\r\n}\r\n</style>\r\n"],"mappings":";;AA2DA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,GAAG,EAAE,oCAAoC;MAAG;MAC5CC,OAAO,EAAE,EAAE;MACXC,WAAW,EAAE,EAAC,CAAE;IAClB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,UAAU,CAAC,CAAC,EAAG;EACtB,CAAC;EACDC,QAAQ,EAAE;IACR;IACAC,eAAeA,CAAA,EAAG;MAChB,OAAO,IAAI,CAACL,OAAO,CAACM,MAAM,CAACN,OAAM,IAAK;QACpC,MAAMO,UAAS,GAAI,IAAI,CAACN,WAAW,CAACO,WAAW,CAAC,CAAC;;QAEjD;QACA,OACGR,OAAO,CAACS,IAAG,IAAKT,OAAO,CAACS,IAAI,CAACD,WAAW,CAAC,CAAC,CAACE,QAAQ,CAACH,UAAU,CAAC,IAC/DP,OAAO,CAACW,KAAI,IAAKX,OAAO,CAACW,KAAK,CAACH,WAAW,CAAC,CAAC,CAACE,QAAQ,CAACH,UAAU,CAAC,IACjEP,OAAO,CAACY,OAAM,IAAKZ,OAAO,CAACY,OAAO,CAACJ,WAAW,CAAC,CAAC,CAACE,QAAQ,CAACH,UAAU,CAAC;MAE1E,CAAC,CAAC;IACJ;EACF,CAAC;EACDM,OAAO,EAAE;IACPV,UAAUA,CAAA,EAAG;MACXN,KAAK,CAACiB,GAAG,CAAC,IAAI,CAACf,GAAG,EACfgB,IAAI,CAACC,QAAO,IAAK;QAChB;QACA,IAAIC,KAAK,CAACC,OAAO,CAACF,QAAQ,CAAClB,IAAI,CAAC,EAAE;UAChC,IAAI,CAACE,OAAM,GAAIgB,QAAQ,CAAClB,IAAI,EAAG;QACjC,OAAO;UACLqB,OAAO,CAACC,KAAK,CAAC,kCAAkC,EAAEJ,QAAQ,CAAClB,IAAI,CAAC;QAClE;MACF,CAAC,EACAuB,KAAK,CAACD,KAAI,IAAK;QACdD,OAAO,CAACC,KAAK,CAAC,oBAAoB,EAAEA,KAAK,CAAC,EAAG;MAC/C,CAAC,CAAC;IACN,CAAC;IACDE,aAAaA,CAACC,SAAS,EAAE;MACvBJ,OAAO,CAACK,GAAG,CAAC,