{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      api: 'http://127.0.0.1:5000/verifikasi',\n      verifikasiList: [],\n      searchQuery: '',\n      perPage: 5,\n      currentPage: 1\n    };\n  },\n  created() {\n    this.fetchVerifikasiList();\n  },\n  computed: {\n    filteredVerifikasi() {\n      return this.verifikasiList.filter(verifikasi => {\n        const searchTerm = this.searchQuery.toLowerCase();\n        return verifikasi.penyewa.nama && verifikasi.penyewa.nama.toLowerCase().includes(searchTerm) || verifikasi.penyewa.email && verifikasi.penyewa.email.toLowerCase().includes(searchTerm) || verifikasi.penyewa.no_telp && verifikasi.penyewa.no_telp.toLowerCase().includes(searchTerm) || verifikasi.kamar.nomor_kamar && verifikasi.kamar.nomor_kamar.toLowerCase().includes(searchTerm);\n      });\n    },\n    totalPages() {\n      return Math.ceil(this.filteredVerifikasi.length / this.perPage);\n    },\n    paginatedVerifikasi() {\n      const start = (this.currentPage - 1) * this.perPage;\n      const end = start + this.perPage;\n      return this.filteredVerifikasi.slice(start, end);\n    }\n  },\n  methods: {\n    fetchVerifikasiList() {\n      axios.get(this.api).then(response => {\n        this.verifikasiList = response.data;\n      }).catch(error => {\n        console.error('Error fetching verifikasi data:', error);\n      });\n    },\n    updateVerifikasi(id, status) {\n      axios.put(`http://127.0.0.1:5000/verifikasi/${id}`, {\n        status_verifikasi: status\n      }).then(() => {\n        this.fetchVerifikasiList(); // Refresh the list after updating\n        alert('Status verifikasi berhasil diperbarui');\n      }).catch(error => {\n        console.error('Error updating verifikasi status:', error);\n        alert('Gagal memperbarui status verifikasi');\n      });\n    },\n    previousPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","api","verifikasiList","searchQuery","perPage","currentPage","created","fetchVerifikasiList","computed","filteredVerifikasi","filter","verifikasi","searchTerm","toLowerCase","penyewa","nama","includes","email","no_telp","kamar","nomor_kamar","totalPages","Math","ceil","length","paginatedVerifikasi","start","end","slice","methods","get","then","response","catch","error","console","updateVerifikasi","id","status","put","status_verifikasi","alert","previousPage","nextPage"],"sources":["D:\\SEMESTER 5\\Proyek\\Web_Kost\\frontend\\src\\views\\VerifikasiView.vue"],"sourcesContent":["<template>\r\n    <div class=\"content\">\r\n      <div class=\"row\">\r\n        <div class=\"col-md-12\">\r\n          <div class=\"card\">\r\n            <div class=\"card-header d-flex justify-content-between\">\r\n              <h4 class=\"card-title\">Verifikasi Reservasi</h4>\r\n            </div>\r\n            <div class=\"card-body\">\r\n              <div class=\"d-flex justify-content-between mb-3\">\r\n                <input \r\n                  v-model=\"searchQuery\" \r\n                  type=\"text\" \r\n                  class=\"form-control ml-auto\" \r\n                  placeholder=\"Cari reservasi...\" \r\n                  style=\"width: 250px;\" \r\n                />\r\n              </div>\r\n              <div class=\"table-responsive\">\r\n                <table class=\"table\">\r\n                  <thead class=\"text-primary\">\r\n                    <tr>\r\n                      <th>No</th>\r\n                      <th>Nama</th>\r\n                      <th>Email</th>\r\n                      <th>No. Telepon</th>\r\n                      <th>Bukti Pembayaran</th>\r\n                      <th>Awal Penyewaan</th>\r\n                      <th>Akhir Penyewaan</th>\r\n                      <th>Nomor Kamar</th>\r\n                      <th>Status Verifikasi</th>\r\n                      <th>Aksi</th>\r\n                    </tr>\r\n                  </thead>\r\n                  <tbody>\r\n                    <tr v-for=\"(verifikasi, index) in paginatedVerifikasi\" :key=\"verifikasi.id\">\r\n                      <td>{{ (currentPage - 1) * perPage + (index + 1) }}</td>\r\n                      <td>{{ verifikasi.penyewa.nama }}</td>\r\n                      <td>{{ verifikasi.penyewa.email }}</td>\r\n                      <td>{{ verifikasi.penyewa.no_telp }}</td>\r\n                      <td>\r\n                        <img v-if=\"verifikasi.reservasi.bukti_pembayaran\" :src=\"verifikasi.reservasi.bukti_pembayaran\" alt=\"Bukti Pembayaran\" width=\"100\" />\r\n                        <span v-else>N/A</span>\r\n                      </td>\r\n                      <td>{{ verifikasi.reservasi.awal_penyewaan }}</td>\r\n                      <td>{{ verifikasi.reservasi.akhir_penyewaan }}</td>\r\n                      <td>{{ verifikasi.kamar.nomor_kamar }}</td>\r\n                      <td>{{ verifikasi.status_verifikasi }}</td>\r\n                      <td class=\"text-right\">\r\n                        <button @click=\"updateVerifikasi(verifikasi.id, 'sudah verifikasi')\" class=\"btn btn-success btn-sm\">Verifikasi</button>\r\n                        <button @click=\"updateVerifikasi(verifikasi.id, 'belum verifikasi')\" class=\"btn btn-warning btn-sm\">Belum Verifikasi</button>\r\n                        <button @click=\"updateVerifikasi(verifikasi.id, 'verifikasi ditolak')\" class=\"btn btn-danger btn-sm\">Verifikasi Ditolak</button>\r\n                      </td>\r\n                    </tr>\r\n                  </tbody>\r\n                </table>\r\n              </div>\r\n  \r\n              <!-- Pagination Controls -->\r\n              <div class=\"pagination-container d-flex justify-content-between mt-3\">\r\n                <span>Halaman {{ currentPage }} dari {{ totalPages }}</span>\r\n                <div class=\"pagination-buttons d-flex justify-content-end\">\r\n                  <button \r\n                    class=\"btn btn-purple btn-sm\" \r\n                    :disabled=\"currentPage === 1\" \r\n                    @click=\"previousPage\"\r\n                  >\r\n                    Previous\r\n                  </button>\r\n                  <button \r\n                    class=\"btn btn-purple btn-sm\" \r\n                    :disabled=\"currentPage === totalPages\" \r\n                    @click=\"nextPage\"\r\n                  >\r\n                    Next\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from 'axios';\r\n  \r\n  export default {\r\n    data() {\r\n      return {\r\n        api: 'http://127.0.0.1:5000/verifikasi',\r\n        verifikasiList: [],\r\n        searchQuery: '',\r\n        perPage: 5,\r\n        currentPage: 1,\r\n      };\r\n    },\r\n    created() {\r\n      this.fetchVerifikasiList();\r\n    },\r\n    computed: {\r\n      filteredVerifikasi() {\r\n        return this.verifikasiList.filter(verifikasi => {\r\n          const searchTerm = this.searchQuery.toLowerCase();\r\n          return (\r\n            (verifikasi.penyewa.nama && verifikasi.penyewa.nama.toLowerCase().includes(searchTerm)) ||\r\n            (verifikasi.penyewa.email && verifikasi.penyewa.email.toLowerCase().includes(searchTerm)) ||\r\n            (verifikasi.penyewa.no_telp && verifikasi.penyewa.no_telp.toLowerCase().includes(searchTerm)) ||\r\n            (verifikasi.kamar.nomor_kamar && verifikasi.kamar.nomor_kamar.toLowerCase().includes(searchTerm))\r\n          );\r\n        });\r\n      },\r\n      totalPages() {\r\n        return Math.ceil(this.filteredVerifikasi.length / this.perPage);\r\n      },\r\n      paginatedVerifikasi() {\r\n        const start = (this.currentPage - 1) * this.perPage;\r\n        const end = start + this.perPage;\r\n        return this.filteredVerifikasi.slice(start, end);\r\n      }\r\n    },\r\n    methods: {\r\n      fetchVerifikasiList() {\r\n        axios.get(this.api)\r\n          .then(response => {\r\n            this.verifikasiList = response.data;\r\n          })\r\n          .catch(error => {\r\n            console.error('Error fetching verifikasi data:', error);\r\n          });\r\n      },\r\n      updateVerifikasi(id, status) {\r\n        axios.put(`http://127.0.0.1:5000/verifikasi/${id}`, { status_verifikasi: status })\r\n          .then(() => {\r\n            this.fetchVerifikasiList(); // Refresh the list after updating\r\n            alert('Status verifikasi berhasil diperbarui');\r\n          })\r\n          .catch(error => {\r\n            console.error('Error updating verifikasi status:', error);\r\n            alert('Gagal memperbarui status verifikasi');\r\n          });\r\n      },\r\n      previousPage() {\r\n        if (this.currentPage > 1) {\r\n          this.currentPage--;\r\n        }\r\n      },\r\n      nextPage() {\r\n        if (this.currentPage < this.totalPages) {\r\n          this.currentPage++;\r\n        }\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .table th, .table td {\r\n    text-align: center;\r\n  }\r\n  \r\n  .table th {\r\n    background-color: #f8f9fa;\r\n    color: #75276d;\r\n  }\r\n  \r\n  .btn-sm {\r\n    margin: 0 5px;\r\n  }\r\n  \r\n  .btn-success {\r\n    background-color: #28a745;\r\n  }\r\n  \r\n  .btn-warning {\r\n    background-color: #ffc107;\r\n  }\r\n  \r\n  .btn-danger {\r\n    background-color: #dc3545;\r\n  }\r\n  \r\n  .btn-purple {\r\n    background-color: #75276d;\r\n    color: white;\r\n  }\r\n  \r\n  .pagination-container {\r\n    display: flex;\r\n    justify-content: space-between;\r\n 