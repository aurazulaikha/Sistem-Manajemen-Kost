{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      user: {\n        id: \"\",\n        nama: \"\",\n        email: \"\",\n        roles: \"\",\n        no_telp: \"\"\n      },\n      notification: {\n        message: \"\",\n        type: \"\"\n      },\n      errors: {}\n    };\n  },\n  created() {\n    const userId = this.$route.params.id;\n    if (userId) {\n      this.fetchUser(userId);\n    }\n  },\n  methods: {\n    async fetchUser(userId) {\n      try {\n        const {\n          data\n        } = await axios.get(`http://127.0.0.1:5000/edit-user/${userId}`);\n        this.user = data;\n      } catch (error) {\n        this.notification.message = \"Gagal mengambil data pengguna.\";\n        this.notification.type = \"error\";\n      }\n    },\n    async submitForm() {\n      this.errors = {};\n      if (!this.user.nama) this.errors.nama = \"Nama tidak boleh kosong.\";\n      if (!this.user.email) this.errors.email = \"Email tidak boleh kosong.\";\n      if (!this.user.no_telp) this.errors.no_telp = \"Nomor telepon tidak boleh kosong.\";\n      if (Object.keys(this.errors).length > 0) return;\n      try {\n        await axios.put(`http://127.0.0.1:5000/edit-user/${this.user.id}`, this.user);\n        this.notification.message = \"Data pengguna berhasil diperbarui.\";\n        this.notification.type = \"success\";\n        setTimeout(() => this.$router.push(\"/list-users\"), 2000);\n      } catch (error) {\n        this.notification.message = \"Gagal memperbarui data pengguna.\";\n        this.notification.type = \"error\";\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","user","id","nama","email","roles","no_telp","notification","message","type","errors","created","userId","$route","params","fetchUser","methods","get","error","submitForm","Object","keys","length","put","setTimeout","$router","push"],"sources":["D:\\SEMESTER 5\\Proyek\\Web_Kost\\frontend\\src\\views\\EditUser.vue"],"sourcesContent":["<template>\r\n  <div class=\"content\">\r\n    <div class=\"col-md-8\">\r\n      <div class=\"card card-user\">\r\n        <div class=\"card-header\">\r\n          <h5 class=\"card-title\">Edit User</h5>\r\n        </div>\r\n        <div class=\"card-body\">\r\n          <!-- Notifikasi -->\r\n          <div v-if=\"notification.message\" :class=\"['alert', notification.type === 'success' ? 'alert-success' : 'alert-danger']\">\r\n            {{ notification.message }}\r\n          </div>\r\n\r\n          <form @submit.prevent=\"submitForm\">\r\n            <!-- Input Nama -->\r\n            <div class=\"form-group\">\r\n              <label>Nama</label>\r\n              <input \r\n                type=\"text\" \r\n                class=\"form-control\" \r\n                placeholder=\"Nama\" \r\n                v-model=\"user.nama\"\r\n                :class=\"{ 'is-invalid': errors.nama }\"\r\n                required\r\n              />\r\n              <div v-if=\"errors.nama\" class=\"invalid-feedback\">{{ errors.nama }}</div>\r\n            </div>\r\n\r\n            <!-- Input Email -->\r\n            <div class=\"form-group\">\r\n              <label>Email</label>\r\n              <input \r\n                type=\"email\" \r\n                class=\"form-control\" \r\n                placeholder=\"Email\" \r\n                v-model=\"user.email\"\r\n                :class=\"{ 'is-invalid': errors.email }\"\r\n                required\r\n              />\r\n              <div v-if=\"errors.email\" class=\"invalid-feedback\">{{ errors.email }}</div>\r\n            </div>\r\n\r\n            <!-- Input Roles -->\r\n            <div class=\"form-group\">\r\n              <label>Roles</label>\r\n              <select \r\n                class=\"form-control\" \r\n                v-model=\"user.roles\" \r\n                required\r\n              >\r\n                <option value=\"admin\">Admin</option>\r\n                <option value=\"penyewa\">Penyewa</option>\r\n                <option value=\"pemilik\">Pemilik</option>\r\n              </select>\r\n            </div>\r\n\r\n            <!-- Input Nomor Telepon -->\r\n            <div class=\"form-group\">\r\n              <label>Nomor Telepon</label>\r\n              <input \r\n                type=\"text\" \r\n                class=\"form-control\" \r\n                placeholder=\"Nomor Telepon\" \r\n                v-model=\"user.no_telp\"\r\n                :class=\"{ 'is-invalid': errors.no_telp }\"\r\n                required\r\n              />\r\n              <div v-if=\"errors.no_telp\" class=\"invalid-feedback\">{{ errors.no_telp }}</div>\r\n            </div>\r\n\r\n            <!-- Tombol Submit -->\r\n            <button type=\"submit\" class=\"btn btn-primary btn-block\">Simpan Perubahan</button>\r\n          </form>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      user: {\r\n        id: \"\",\r\n        nama: \"\",\r\n        email: \"\",\r\n        roles: \"\",\r\n        no_telp: \"\",\r\n      },\r\n      notification: {\r\n        message: \"\",\r\n        type: \"\",\r\n      },\r\n      errors: {},\r\n    };\r\n  },\r\n  created() {\r\n    const userId = this.$route.params.id;\r\n    if (userId) {\r\n      this.fetchUser(userId);\r\n    }\r\n  },\r\n  methods: {\r\n    async fetchUser(userId) {\r\n      try {\r\n        const { data } = await axios.get(`http://127.0.0.1:5000/edit-user/${userId}`);\r\n        this.user = data;\r\n      } catch (error) {\r\n        this.notification.message = \"Gagal mengambil data pengguna.\";\r\n        this.notification.type = \"error\";\r\n      }\r\n    },\r\n    async submitForm() {\r\n      this.errors = {};\r\n      if (!this.user.nama) this.errors.nama = \"Nama tidak boleh kosong.\";\r\n      if (!this.user.email) this.errors.email = \"Email tidak boleh kosong.\";\r\n      if (!this.user.no_telp) this.errors.no_telp = \"Nomor telepon tidak boleh kosong.\";\r\n\r\n      if (Object.keys(this.errors).length > 0) return;\r\n\r\n      try {\r\n        await axios.put(`http://127.0.0.1:5000/edit-user/${this.user.id}`, this.user);\r\n        this.notification.message = \"Data pengguna berhasil diperbarui.\";\r\n        this.notification.type = \"success\";\r\n        setTimeout(() => this.$router.push(\"/list-users\"), 2000);\r\n      } catch (error) {\r\n        this.notification.message = \"Gagal memperbarui data pengguna.\";\r\n        this.notification.type = \"error\";\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.invalid-feedback {\r\n  color: red;\r\n}\r\n.alert {\r\n  margin-top: 10px;\r\n}\r\n</style>\r\n"],"mappings":";AAgFA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,EAAE,EAAE,EAAE;QACNC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE,EAAE;QACTC,KAAK,EAAE,EAAE;QACTC,OAAO,EAAE;MACX,CAAC;MACDC,YAAY,EAAE;QACZC,OAAO,EAAE,EAAE;QACXC,IAAI,EAAE;MACR,CAAC;MACDC,MAAM,EAAE,CAAC;IACX,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,MAAMC,MAAK,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACZ,EAAE;IACpC,IAAIU,MAAM,EAAE;MACV,IAAI,CAACG,SAAS,CAACH,MAAM,CAAC;IACxB;EACF,CAAC;EACDI,OAAO,EAAE;IACP,MAAMD,SAASA,CAACH,MAAM,EAAE;MACtB,IAAI;QACF,MAAM;UAAEZ;QAAK,IAAI,MAAMD,KAAK,CAACkB,GAAG,CAAC,mCAAmCL,MAAM,EAAE,CAAC;QAC7E,IAAI,CAACX,IAAG,GAAID,IAAI;MAClB,EAAE,OAAOkB,KAAK,EAAE;QACd,IAAI,CAACX,YAAY,CAACC,OAAM,GAAI,gCAAgC;QAC5D,IAAI,CAACD,YAAY,CAACE,IAAG,GAAI,OAAO;MAClC;IACF,CAAC;IACD,MAAMU,UAAUA,CAAA,EAAG;MACjB,IAAI,CAACT,MAAK,GAAI,CAAC,CAAC;MAChB,IAAI,CAAC,IAAI,CAACT,IAAI,CAACE,IAAI,EAAE,IAAI,CAACO,MAAM,CAACP,IAAG,GAAI,0BAA0B;MAClE,IAAI,CAAC,IAAI,CAACF,IAAI,CAACG,KAAK,EAAE,IAAI,CAACM,MAAM,CAACN,KAAI,GAAI,2BAA2B;MACrE,IAAI,CAAC,IAAI,CAACH,IAAI,CAACK,OAAO,EAAE,IAAI,CAACI,MAAM,CAACJ,OAAM,GAAI,mCAAmC;MAEjF,IAAIc,MAAM,CAACC,IAAI,CAAC,IAAI,CAACX,MAAM,CAAC,CAACY,MAAK,GAAI,