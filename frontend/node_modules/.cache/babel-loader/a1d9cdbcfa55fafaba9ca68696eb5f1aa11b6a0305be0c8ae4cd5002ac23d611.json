{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      api: 'http://127.0.0.1:5000/list-penyewa',\n      // API untuk daftar penyewa\n      penyewa: [],\n      // Daftar penyewa\n      searchQuery: '',\n      // Query pencarian\n      perPage: 5,\n      // Jumlah per halaman\n      currentPage: 1,\n      // Halaman saat ini\n      showDeleteModal: false,\n      // Status modal hapus\n      penyewaToDelete: null // Penyewa yang akan dihapus\n    };\n  },\n  created() {\n    this.getPenyewa(); // Ambil data penyewa saat komponen dibuat\n  },\n  computed: {\n    filteredPenyewa() {\n      return this.penyewa.filter(penyewa => {\n        const searchTerm = this.searchQuery.toLowerCase();\n        return penyewa.nama && penyewa.nama.toLowerCase().includes(searchTerm) || penyewa.email && penyewa.email.toLowerCase().includes(searchTerm) || penyewa.no_telp && penyewa.no_telp.toLowerCase().includes(searchTerm);\n      });\n    },\n    totalPages() {\n      return Math.ceil(this.filteredPenyewa.length / this.perPage); // Total halaman\n    },\n    paginatedPenyewa() {\n      const start = (this.currentPage - 1) * this.perPage;\n      const end = start + this.perPage;\n      return this.filteredPenyewa.slice(start, end); // Data penyewa yang ditampilkan per halaman\n    }\n  },\n  methods: {\n    getPenyewa() {\n      axios.get(this.api).then(response => {\n        if (Array.isArray(response.data)) {\n          this.penyewa = response.data; // Simpan data penyewa dari response\n        } else {\n          console.error('Data yang diterima tidak sesuai:', response.data);\n        }\n      }).catch(error => {\n        console.error('Terjadi kesalahan:', error);\n      });\n    },\n    confirmDelete(penyewa) {\n      this.penyewaToDelete = penyewa; // Set penyewa yang akan dihapus\n      this.showDeleteModal = true; // Tampilkan modal konfirmasi\n    },\n    closeDeleteModal() {\n      this.showDeleteModal = false; // Tutup modal\n      this.penyewaToDelete = null; // Reset penyewa yang akan dihapus\n    },\n    deletePenyewa() {\n      if (this.penyewaToDelete && this.penyewaToDelete.id) {\n        axios.delete(`http://127.0.0.1:5000/delete-penyewa/${this.penyewaToDelete.id}`).then(response => {\n          console.log('Penyewa berhasil dihapus:', response);\n          this.getPenyewa(); // Refresh daftar penyewa\n          this.closeDeleteModal(); // Tutup modal\n        }).catch(error => {\n          console.error('Terjadi kesalahan saat menghapus penyewa:', error);\n        });\n      } else {\n        console.error('ID penyewa tidak ditemukan');\n      }\n    },\n    previousPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--; // Pindah ke halaman sebelumnya\n      }\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++; // Pindah ke halaman berikutnya\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","api","penyewa","searchQuery","perPage","currentPage","showDeleteModal","penyewaToDelete","created","getPenyewa","computed","filteredPenyewa","filter","searchTerm","toLowerCase","nama","includes","email","no_telp","totalPages","Math","ceil","length","paginatedPenyewa","start","end","slice","methods","get","then","response","Array","isArray","console","error","catch","confirmDelete","closeDeleteModal","deletePenyewa","id","delete","log","previousPage","nextPage"],"sources":["D:\\SEMESTER 5\\Proyek\\Web_Kost\\frontend\\src\\views\\ListPenyewa.vue"],"sourcesContent":["<template>\r\n  <div class=\"content\">\r\n    <div class=\"row\">\r\n      <div class=\"col-md-12\">\r\n        <div class=\"card\">\r\n          <div class=\"card-header d-flex justify-content-between\">\r\n            <h4 class=\"card-title\">Daftar Penyewa</h4>\r\n          </div>\r\n          <div class=\"card-body\">\r\n            <div class=\"d-flex justify-content-between mb-3\">\r\n              <router-link to=\"/tambah-penyewa\">\r\n                <button class=\"btn btn-purple btn-sm\">Tambah Data</button>\r\n              </router-link>\r\n              <input \r\n                v-model=\"searchQuery\" \r\n                type=\"text\" \r\n                class=\"form-control\" \r\n                placeholder=\"Cari penyewa...\" \r\n                style=\"width: 250px;\" \r\n              />\r\n            </div>\r\n            <div class=\"table-responsive\">\r\n              <table class=\"table\">\r\n                <thead class=\"text-primary\">\r\n                  <tr>\r\n                    <th>No</th>\r\n                    <th>Nama</th>\r\n                    <th>Email</th>\r\n                    <th>Phone</th>\r\n                    <th>Action</th>\r\n                  </tr>\r\n                </thead>\r\n                <tbody>\r\n                  <tr v-for=\"(penyewa, index) in paginatedPenyewa\" :key=\"penyewa.id\">\r\n                    <td>{{ (currentPage - 1) * perPage + (index + 1) }}</td>\r\n                    <td>{{ penyewa.nama }}</td>\r\n                    <td>{{ penyewa.email }}</td>\r\n                    <td>{{ penyewa.no_telp }}</td>\r\n                    <td class=\"text-right\">\r\n                      <router-link :to=\"'/edit-penyewa/' + penyewa.id\">\r\n                        <button class=\"btn btn-warning btn-sm\">Edit</button>\r\n                      </router-link>\r\n                      <button class=\"btn btn-danger btn-sm\" @click=\"confirmDelete(penyewa)\">Hapus</button>\r\n                    </td>\r\n                  </tr>\r\n                </tbody>\r\n              </table>\r\n            </div>\r\n            \r\n            <!-- Paginasi -->\r\n            <div class=\"pagination-container d-flex justify-content-between mt-3\">\r\n              <span>Halaman {{ currentPage }} dari {{ totalPages }}</span>\r\n              <div class=\"pagination-buttons d-flex justify-content-end\">\r\n                <button \r\n                  class=\"btn btn-purple btn-sm\" \r\n                  :disabled=\"currentPage === 1\" \r\n                  @click=\"previousPage\"\r\n                >\r\n                  Previous\r\n                </button>\r\n                <button \r\n                  class=\"btn btn-purple btn-sm\" \r\n                  :disabled=\"currentPage === totalPages\" \r\n                  @click=\"nextPage\"\r\n                >\r\n                  Next\r\n                </button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Modal Konfirmasi Penghapusan -->\r\n    <div v-if=\"showDeleteModal\" class=\"modal fade show\" tabindex=\"-1\" style=\"display: block;\" aria-modal=\"true\">\r\n      <div class=\"modal-dialog\">\r\n        <div class=\"modal-content\">\r\n          <div class=\"modal-header\">\r\n            <h5 class=\"modal-title\">Konfirmasi Penghapusan</h5>\r\n            <button type=\"button\" class=\"close\" @click=\"closeDeleteModal\">\r\n              <span aria-hidden=\"true\">&times;</span>\r\n            </button>\r\n          </div>\r\n          <div class=\"modal-body\">\r\n            <p>Apakah Anda yakin ingin menghapus penyewa <strong>{{ penyewaToDelete.nama }}</strong>?</p>\r\n          </div>\r\n          <div class=\"modal-footer\">\r\n            <button type=\"button\" class=\"btn btn-secondary\" @click=\"closeDeleteModal\">Batal</button>\r\n            <button type=\"button\" class=\"btn btn-danger\" @click=\"deletePenyewa\">Hapus</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      api: 'http://127.0.0.1:5000/list-penyewa',  // API untuk daftar penyewa\r\n      penyewa: [],  // Daftar penyewa\r\n      searchQuery: '',  // Query pencarian\r\n      perPage: 5,  // Jumlah per halaman\r\n      currentPage: 1,  // Halaman saat ini\r\n      showDeleteModal: false,  // Status modal hapus\r\n      penyewaToDelete: null,  // Penyewa yang akan dihapus\r\n    };\r\n  },\r\n  created() {\r\n    this.getPenyewa();  // Ambil data penyewa saat komponen dibuat\r\n  },\r\n  computed: {\r\n    filteredPenyewa() {\r\n      return this.penyewa.filter(penyewa => {\r\n        const searchTerm = this.searchQuery.toLowerCase();\r\n        return (\r\n          (penyewa.nama && penyewa.nama.toLowerCase().includes(searchTerm)) ||\r\n          (penyewa.email && penyewa.email.toLowerCase().includes(searchTerm)) ||\r\n          (penyewa.no_telp && penyewa.no_telp.toLowerCase().includes(searchTerm))\r\n        );\r\n      });\r\n    },\r\n    totalPages() {\r\n      return Math.ceil(this.filteredPenyewa.length / this.perPage);  // Total halaman\r\n    },\r\n    paginatedPenyewa() {\r\n      const start = (this.currentPage - 1) * this.perPage;\r\n      const end = start + this.perPage;\r\n      return this.filteredPenyewa.slice(start, end);  // Data penyewa yang ditampilkan per halaman\r\n    }\r\n  },\r\n  methods: {\r\n    getPenyewa() {\r\n      axios.get(this.api)\r\n        .then(response => {\r\n          if (Array.isArray(response.data)) {\r\n            this.penyewa = response.data;  // Simpan data penyewa dari response\r\n          } else {\r\n            console.error('Data yang diterima tidak sesuai:', response.data);\r\n          }\r\n        })\r\n        .catch(error => {\r\n          console.error('Terjadi kesalahan:', error); \r\n        });\r\n    },\r\n    confirmDelete(penyewa) {\r\n      this.penyewaToDelete = penyewa;  // Set penyewa yang akan dihapus\r\n      this.showDeleteModal = true;  // Tampilkan modal konfirmasi\r\n    },\r\n    closeDeleteModal() {\r\n      this.showDeleteModal = false;  // Tutup modal\r\n      this.penyewaToDelete = null;  // Reset penyewa yang akan dihapus\r\n    },\r\n    deletePenyewa() {\r\n      if (this.penyewaToDelete && this.penyewaToDelete.id) {\r\n        axios.delete(`http://127.0.0.1:5000/delete-penyewa/${this.penyewaToDelete.id}`)\r\n          .then(response => {\r\n            console.log('Penyewa berhasil dihapus:', response);\r\n            this.getPenyewa();  // Refresh daftar penyewa\r\n            this.closeDeleteModal();  // Tutup modal\r\n          })\r\n          .catch(error => {\r\n            console.error('Terjadi kesalahan saat menghapus penyewa:', error);\r\n          });\r\n      } else {\r\n        console.error('ID penyewa tidak ditemukan');\r\n      }\r\n    },\r\n    previousPage() {\r\n      if (this.currentPage > 1) {\r\n        this.currentPage--;  // Pindah ke halaman sebelumnya\r\n      }\r\n    },\r\n    nextPage() {\r\n      if (this.currentPage < this.totalPages) {\r\n        this.currentPage++;  // Pindah ke halaman berikutnya\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.table th, .table td {\r\n  text-align: center;\r\n}\r\n\r\n.table th {\r\n  background-color: #f8f9fa;\r\n}\r\n\r\n.btn-sm {\r\n  margin: 0 5px;\r\n}\r\n\r\n.btn-warning {\r\n  background-color: #ffc107;\r\n}\r\n\r\n.btn-danger {\r\n  background-color: #dc3545;\r\n}\r\n\r\n.btn-purple {\r\n  background-color: #75276d; \r\n  color: white;\r\n}\r\n\r\n.btn-purple:hover {\r\n  background-color: #5e1d57; \r\n}\r\n\r\n/* Paginasi */\r\n.pagination-container {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.pagination-buttons {\r\n  display: flex;\r\n  gap: 10px;\r\n}\r\n\r\n/* Modal Konfirmasi */\r\n.modal-backdrop {\r\n  z-index: 1040;\r\n}\r\n</style>\r\n"],"mappings":";;AAkGA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,GAAG,EAAE,oCAAoC;MAAG;MAC5CC,OAAO,EAAE,EAAE;MAAG;MACdC,WAAW,EAAE,EAAE;MAAG;MAClBC,OAAO,EAAE,CAAC;MAAG;MACbC,WAAW,EAAE,CAAC;MAAG;MACjBC,eAAe,EAAE,KAAK;MAAG;MACzBC,eAAe,EAAE,IAAI,CAAG;IAC1B,CAAC