{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      api: 'http://127.0.0.1:5000/list-user',\n      // Endpoint API yang benar\n      users: [],\n      searchQuery: '' // Untuk fitur pencarian\n    };\n  },\n  created() {\n    this.getUsers(); // Mengambil data user saat created\n  },\n  computed: {\n    // Filter users berdasarkan pencarian yang ada di searchQuery\n    filteredUsers() {\n      return this.users.filter(user => {\n        const searchTerm = this.searchQuery.toLowerCase();\n\n        // Menambahkan pengecekan apakah properti `user.id`, `user.nama`, dll ada dan bukan undefined\n        return user.id && user.id.toString().includes(searchTerm) || user.nama && user.nama.toLowerCase().includes(searchTerm) || user.email && user.email.toLowerCase().includes(searchTerm) || user.roles && user.roles.toLowerCase().includes(searchTerm) || user.no_telp && user.no_telp.toLowerCase().includes(searchTerm);\n      });\n    }\n  },\n  methods: {\n    getUsers() {\n      axios.get(this.api).then(response => {\n        // Pastikan data yang diterima memiliki format yang benar\n        if (Array.isArray(response.data)) {\n          this.users = response.data; // Simpan data ke dalam state 'users'\n        } else {\n          console.error('Data yang diterima tidak sesuai:', response.data);\n        }\n      }).catch(error => {\n        console.error('Terjadi kesalahan:', error); // Cek error jika ada masalah dalam request\n      });\n    },\n    confirmDelete(userId) {\n      console.log(`User dengan ID ${userId} akan dihapus`); // Logika hapus bisa ditambahkan di sini\n    }\n  }\n};","map":{"version":3,"names":["axios","data","api","users","searchQuery","created","getUsers","computed","filteredUsers","filter","user","searchTerm","toLowerCase","id","toString","includes","nama","email","roles","no_telp","methods","get","then","response","Array","isArray","console","error","catch","confirmDelete","userId","log"],"sources":["D:\\SEMESTER 5\\Proyek\\Web_Kost\\frontend\\src\\views\\ListUser.vue"],"sourcesContent":["<template>\r\n  <div class=\"content\">\r\n    <div class=\"row\">\r\n      <div class=\"col-md-12\">\r\n        <div class=\"card\">\r\n          <div class=\"card-header d-flex justify-content-between\">\r\n            <h4 class=\"card-title\">Daftar User</h4>\r\n            <div>\r\n              <!-- Button Tambah User -->\r\n              <router-link to=\"/tambah-user\">\r\n                <button class=\"btn btn-primary btn-sm\">Tambah User</button>\r\n              </router-link>\r\n              <!-- Search Input -->\r\n              <input \r\n                v-model=\"searchQuery\" \r\n                type=\"text\" \r\n                class=\"form-control\" \r\n                placeholder=\"Cari user...\" \r\n                style=\"width: 250px;\" \r\n              />\r\n            </div>\r\n          </div>\r\n          <div class=\"card-body\">\r\n            <div class=\"table-responsive\">\r\n              <table class=\"table\">\r\n                <thead class=\"text-primary\">\r\n                  <tr>\r\n                    <th>No</th>\r\n                    <th>Nama</th>\r\n                    <th>Email</th>\r\n                    <th>Role</th>\r\n                    <th>Phone</th>\r\n                    <th>Action</th>\r\n                  </tr>\r\n                </thead>\r\n                <tbody>\r\n                  <!-- Loop through the filteredUsers array to display users with filter applied -->\r\n                  <tr v-for=\"(user, index) in filteredUsers\" :key=\"user.id\">\r\n                    <td>{{ index + 1 }}</td>\r\n                    <td>{{ user.nama }}</td>\r\n                    <td>{{ user.email }}</td>\r\n                    <td>{{ user.roles }}</td>\r\n                    <td>{{ user.no_telp }}</td>\r\n                    <td class=\"text-right\">\r\n                      <router-link :to=\"'/edit-user/' + user.id\">\r\n                        <button class=\"btn btn-warning btn-sm\">Edit</button>\r\n                      </router-link>\r\n                      <button class=\"btn btn-danger btn-sm\" @click=\"confirmDelete(user.id)\">Hapus</button>\r\n                    </td>\r\n                  </tr>\r\n                </tbody>\r\n              </table>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      api: 'http://127.0.0.1:5000/list-user',  // Endpoint API yang benar\r\n      users: [],\r\n      searchQuery: '' // Untuk fitur pencarian\r\n    };\r\n  },\r\n  created() {\r\n    this.getUsers();  // Mengambil data user saat created\r\n  },\r\n  computed: {\r\n    // Filter users berdasarkan pencarian yang ada di searchQuery\r\n    filteredUsers() {\r\n      return this.users.filter(user => {\r\n        const searchTerm = this.searchQuery.toLowerCase();\r\n\r\n        // Menambahkan pengecekan apakah properti `user.id`, `user.nama`, dll ada dan bukan undefined\r\n        return (\r\n          (user.id && user.id.toString().includes(searchTerm)) || \r\n          (user.nama && user.nama.toLowerCase().includes(searchTerm)) ||\r\n          (user.email && user.email.toLowerCase().includes(searchTerm)) ||\r\n          (user.roles && user.roles.toLowerCase().includes(searchTerm)) ||\r\n          (user.no_telp && user.no_telp.toLowerCase().includes(searchTerm))\r\n        );\r\n      });\r\n    }\r\n  },\r\n  methods: {\r\n    getUsers() {\r\n      axios.get(this.api)\r\n        .then(response => {\r\n          // Pastikan data yang diterima memiliki format yang benar\r\n          if (Array.isArray(response.data)) {\r\n            this.users = response.data;  // Simpan data ke dalam state 'users'\r\n          } else {\r\n            console.error('Data yang diterima tidak sesuai:', response.data);\r\n          }\r\n        })\r\n        .catch(error => {\r\n          console.error('Terjadi kesalahan:', error);  // Cek error jika ada masalah dalam request\r\n        });\r\n    },\r\n    confirmDelete(userId) {\r\n      console.log(`User dengan ID ${userId} akan dihapus`);  // Logika hapus bisa ditambahkan di sini\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* Styling untuk tabel dan tombol */\r\n.table th, .table td {\r\n  text-align: center;\r\n}\r\n\r\n.table th {\r\n  background-color: #f8f9fa;\r\n}\r\n\r\n.btn-sm {\r\n  margin: 0 5px;\r\n}\r\n\r\n.btn-warning {\r\n  background-color: #ffc107;\r\n}\r\n\r\n.btn-danger {\r\n  background-color: #dc3545;\r\n}\r\n\r\n.btn-primary {\r\n  margin-right: 10px;\r\n}\r\n</style>\r\n"],"mappings":";;AA6DA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,GAAG,EAAE,iCAAiC;MAAG;MACzCC,KAAK,EAAE,EAAE;MACTC,WAAW,EAAE,EAAC,CAAE;IAClB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,QAAQ,CAAC,CAAC,EAAG;EACpB,CAAC;EACDC,QAAQ,EAAE;IACR;IACAC,aAAaA,CAAA,EAAG;MACd,OAAO,IAAI,CAACL,KAAK,CAACM,MAAM,CAACC,IAAG,IAAK;QAC/B,MAAMC,UAAS,GAAI,IAAI,CAACP,WAAW,CAACQ,WAAW,CAAC,CAAC;;QAEjD;QACA,OACGF,IAAI,CAACG,EAAC,IAAKH,IAAI,CAACG,EAAE,CAACC,QAAQ,CAAC,CAAC,CAACC,QAAQ,CAACJ,UAAU,CAAC,IAClDD,IAAI,CAACM,IAAG,IAAKN,IAAI,CAACM,IAAI,CAACJ,WAAW,CAAC,CAAC,CAACG,QAAQ,CAACJ,UAAU,CAAC,IACzDD,IAAI,CAACO,KAAI,IAAKP,IAAI,CAACO,KAAK,CAACL,WAAW,CAAC,CAAC,CAACG,QAAQ,CAACJ,UAAU,CAAC,IAC3DD,IAAI,CAACQ,KAAI,IAAKR,IAAI,CAACQ,KAAK,CAACN,WAAW,CAAC,CAAC,CAACG,QAAQ,CAACJ,UAAU,CAAC,IAC3DD,IAAI,CAACS,OAAM,IAAKT,IAAI,CAACS,OAAO,CAACP,WAAW,CAAC,CAAC,CAACG,QAAQ,CAACJ,UAAU,CAAC;MAEpE,CAAC,CAAC;IACJ;EACF,CAAC;EACDS,OAAO,EAAE;IACPd,QAAQA,CAAA,EAAG;MACTN,KAAK,CAACqB,GAAG,CAAC,IAAI,CAACnB,GAAG,EACfoB,IAAI,CAACC,QAAO,IAAK;QAChB;QACA,IAAIC,KAAK,CAACC,OAAO,CAACF,QAAQ,CAACtB,IAAI,CAAC,EAAE;UAChC,IAAI,CAACE,KAAI,GAAIoB,QAAQ,CAACtB,IAAI,EAAG;QAC/B,OAAO;UACLyB,O