{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      formData: {\n        kamar_id: this.$route.params.kamarId,\n        nama: '',\n        email: '',\n        periode_penyewaan: '',\n        awal_penyewaan: '',\n        akhir_penyewaan: '',\n        bukti_pembayaran: null,\n        jumlah_harga: null\n      },\n      notification: {\n        message: '',\n        type: ''\n      },\n      errors: {\n        nama: '',\n        email: '',\n        periode_penyewaan: '',\n        awal_penyewaan: '',\n        akhir_penyewaan: '',\n        jumlah_harga: '',\n        bukti_pembayaran: ''\n      }\n    };\n  },\n  methods: {\n    handleFileUpload(event) {\n      const file = event.target.files[0];\n      if (file) {\n        // Validasi ekstensi file gambar\n        const validImageTypes = ['image/jpeg', 'image/png', 'image/jpg'];\n        if (validImageTypes.includes(file.type)) {\n          this.formData.bukti_pembayaran = file;\n          this.errors.bukti_pembayaran = ''; // Reset error jika file valid\n        } else {\n          this.errors.bukti_pembayaran = 'Hanya file gambar (JPEG, PNG, JPG) yang diperbolehkan';\n          this.formData.bukti_pembayaran = null; // Hapus file jika tidak valid\n        }\n      }\n    },\n    async submitReservasi() {\n      // Reset errors and notification\n      this.errors = {};\n      this.notification = {};\n\n      // Validasi form\n      if (!this.formData.nama) {\n        this.errors.nama = 'Nama tidak boleh kosong';\n      }\n      if (!this.formData.email) {\n        this.errors.email = 'Email tidak boleh kosong';\n      } else if (!this.isValidEmail(this.formData.email)) {\n        this.errors.email = 'Format email tidak valid';\n      }\n      if (!this.formData.periode_penyewaan) {\n        this.errors.periode_penyewaan = 'Periode penyewaan tidak boleh kosong';\n      }\n      if (!this.formData.awal_penyewaan) {\n        this.errors.awal_penyewaan = 'Tanggal awal penyewaan tidak boleh kosong';\n      }\n      if (!this.formData.akhir_penyewaan) {\n        this.errors.akhir_penyewaan = 'Tanggal akhir penyewaan tidak boleh kosong';\n      }\n      if (!this.formData.jumlah_harga) {\n        this.errors.jumlah_harga = 'Jumlah harga tidak boleh kosong';\n      }\n      if (!this.formData.bukti_pembayaran) {\n        this.errors.bukti_pembayaran = 'Bukti pembayaran tidak boleh kosong';\n      }\n\n      // Jika ada error, jangan kirim form\n      if (Object.keys(this.errors).length > 0) {\n        return;\n      }\n\n      // Kirim data ke server\n      const formDataToSend = new FormData();\n      formDataToSend.append('kamar_id', this.formData.kamar_id);\n      formDataToSend.append('nama', this.formData.nama);\n      formDataToSend.append('email', this.formData.email);\n      formDataToSend.append('periode_penyewaan', this.formData.periode_penyewaan);\n      formDataToSend.append('awal_penyewaan', this.formData.awal_penyewaan);\n      formDataToSend.append('akhir_penyewaan', this.formData.akhir_penyewaan);\n      formDataToSend.append('bukti_pembayaran', this.formData.bukti_pembayaran);\n      formDataToSend.append('jumlah_harga', this.formData.jumlah_harga);\n      try {\n        const response = await axios.post('http://127.0.0.1:5000/reservasi', formDataToSend, {\n          headers: {\n            'Content-Type': 'multipart/form-data'\n          }\n        });\n        this.notification = {\n          message: response.data.message,\n          type: 'success'\n        };\n        this.$router.push('/'); // Redirect ke halaman utama setelah sukses\n      } catch (error) {\n        console.error('Error submitting reservasi:', error);\n        this.notification = {\n          message: 'Terjadi kesalahan saat mengirim reservasi.',\n          type: 'error'\n        };\n      }\n    },\n    // Cek format email valid\n    isValidEmail(email) {\n      const regex = /^[a-zA-Z0-9._-]+@[a-zA-Z0-9.-]