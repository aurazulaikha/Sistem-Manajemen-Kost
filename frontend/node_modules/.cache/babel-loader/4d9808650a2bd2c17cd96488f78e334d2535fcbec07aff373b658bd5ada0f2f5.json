{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      api: 'http://127.0.0.1:5000/list-pemilik',\n      pemilik: [],\n      searchQuery: ''\n    };\n  },\n  created() {\n    this.getPemilik();\n  },\n  computed: {\n    filteredPemilik() {\n      return this.pemilik.filter(pemilik => {\n        const searchTerm = this.searchQuery.toLowerCase();\n        return pemilik.nama && pemilik.nama.toLowerCase().includes(searchTerm) || pemilik.email && pemilik.email.toLowerCase().includes(searchTerm) || pemilik.no_telp && pemilik.no_telp.toLowerCase().includes(searchTerm);\n      });\n    }\n  },\n  methods: {\n    getPemilik() {\n      axios.get(this.api).then(response => {\n        if (Array.isArray(response.data)) {\n          this.pemilik = response.data;\n        } else {\n          console.error('Data yang diterima tidak sesuai:', response.data);\n        }\n      }).catch(error => {\n        console.error('Terjadi kesalahan:', error);\n      });\n    },\n    confirmDelete(pemilikId) {\n      console.log(`Pemilik dengan ID ${pemilikId} akan dihapus`);\n    }\n  }\n};","map":{"version":3,"names":["axios","data","api","pemilik","searchQuery","created","getPemilik","computed","filteredPemilik","filter","searchTerm","toLowerCase","nama","includes","email","no_telp","methods","get","then","response","Array","isArray","console","error","catch","confirmDelete","pemilikId","log"],"sources":["D:\\SEMESTER 5\\Proyek\\Web_Kost\\frontend\\src\\views\\ListPemilik.vue"],"sourcesContent":["<template>\r\n  <div class=\"content\">\r\n    <div class=\"row\">\r\n      <div class=\"col-md-12\">\r\n        <div class=\"card\">\r\n          <div class=\"card-header d-flex justify-content-between\">\r\n            <h4 class=\"card-title\">Daftar Pemilik</h4>\r\n          </div>\r\n          <div class=\"card-body\">\r\n            <div class=\"d-flex justify-content-between mb-3\">\r\n              <router-link to=\"/tambah-pemilik\">\r\n                <button class=\"btn btn-purple btn-sm\">Tambah Data</button>\r\n              </router-link>\r\n              <!-- Search Input -->\r\n              <input \r\n                v-model=\"searchQuery\" \r\n                type=\"text\" \r\n                class=\"form-control\" \r\n                placeholder=\"Cari pemilik...\" \r\n                style=\"width: 250px;\" \r\n              />\r\n            </div>\r\n            <div class=\"table-responsive\">\r\n              <table class=\"table\">\r\n                <thead class=\"text-primary\">\r\n                  <tr>\r\n                    <th>No</th>\r\n                    <th>Nama</th>\r\n                    <th>Email</th>\r\n                    <th>Phone</th>\r\n                    <th>Action</th>\r\n                  </tr>\r\n                </thead>\r\n                <tbody>\r\n                  <tr v-for=\"(pemilik, index) in filteredPemilik\" :key=\"pemilik.id\">\r\n                    <td>{{ index + 1 }}</td>\r\n                    <td>{{ pemilik.nama }}</td>\r\n                    <td>{{ pemilik.email }}</td>\r\n                    <td>{{ pemilik.no_telp }}</td>\r\n                    <td class=\"text-right\">\r\n                      <router-link :to=\"'/edit-pemilik/' + pemilik.id\">\r\n                        <button class=\"btn btn-warning btn-sm\">Edit</button>\r\n                      </router-link>\r\n                      <button class=\"btn btn-danger btn-sm\" @click=\"confirmDelete(pemilik.id)\">Hapus</button>\r\n                    </td>\r\n                  </tr>\r\n                </tbody>\r\n              </table>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      api: 'http://127.0.0.1:5000/list-pemilik',  \r\n      pemilik: [],  \r\n      searchQuery: '' \r\n    };\r\n  },\r\n  created() {\r\n    this.getPemilik();  \r\n  },\r\n  computed: {\r\n    filteredPemilik() {\r\n      return this.pemilik.filter(pemilik => {\r\n        const searchTerm = this.searchQuery.toLowerCase();\r\n        return (\r\n          (pemilik.nama && pemilik.nama.toLowerCase().includes(searchTerm)) ||\r\n          (pemilik.email && pemilik.email.toLowerCase().includes(searchTerm)) ||\r\n          (pemilik.no_telp && pemilik.no_telp.toLowerCase().includes(searchTerm))\r\n        );\r\n      });\r\n    }\r\n  },\r\n  methods: {\r\n    getPemilik() {\r\n      axios.get(this.api)\r\n        .then(response => {\r\n          if (Array.isArray(response.data)) {\r\n            this.pemilik = response.data; \r\n          } else {\r\n            console.error('Data yang diterima tidak sesuai:', response.data);\r\n          }\r\n        })\r\n        .catch(error => {\r\n          console.error('Terjadi kesalahan:', error); \r\n        });\r\n    },\r\n    confirmDelete(pemilikId) {\r\n      console.log(`Pemilik dengan ID ${pemilikId} akan dihapus`); \r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.table th, .table td {\r\n  text-align: center;\r\n}\r\n\r\n.table th {\r\n  background-color: #f8f9fa;\r\n}\r\n\r\n.btn-sm {\r\n  margin: 0 5px;\r\n}\r\n\r\n.btn-warning {\r\n  background-color: #ffc107;\r\n}\r\n\r\n.btn-danger {\r\n  background-color: #dc3545;\r\n}\r\n\r\n.btn-purple {\r\n  background-color: #75276d; \r\n  color: white;\r\n}\r\n\r\n.btn-purple:hover {\r\n  background-color: #5e1d57; \r\n}\r\n</style>\r\n"],"mappings":";;AAyDA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,GAAG,EAAE,oCAAoC;MACzCC,OAAO,EAAE,EAAE;MACXC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,UAAU,CAAC,CAAC;EACnB,CAAC;EACDC,QAAQ,EAAE;IACRC,eAAeA,CAAA,EAAG;MAChB,OAAO,IAAI,CAACL,OAAO,CAACM,MAAM,CAACN,OAAM,IAAK;QACpC,MAAMO,UAAS,GAAI,IAAI,CAACN,WAAW,CAACO,WAAW