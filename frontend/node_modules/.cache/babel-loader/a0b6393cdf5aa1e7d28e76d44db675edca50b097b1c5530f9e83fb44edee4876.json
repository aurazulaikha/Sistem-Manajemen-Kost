{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      api: 'http://127.0.0.1:5000/list-penyewa',\n      penyewa: [],\n      searchQuery: ''\n    };\n  },\n  created() {\n    this.getPenyewa();\n  },\n  computed: {\n    filteredPenyewa() {\n      return this.penyewa.filter(penyewa => {\n        const searchTerm = this.searchQuery.toLowerCase();\n        return penyewa.nama && penyewa.nama.toLowerCase().includes(searchTerm) || penyewa.email && penyewa.email.toLowerCase().includes(searchTerm) || penyewa.no_telp && penyewa.no_telp.toLowerCase().includes(searchTerm);\n      });\n    }\n  },\n  methods: {\n    getPenyewa() {\n      axios.get(this.api).then(response => {\n        if (Array.isArray(response.data)) {\n          this.penyewa = response.data;\n        } else {\n          console.error('Data yang diterima tidak sesuai:', response.data);\n        }\n      }).catch(error => {\n        console.error('Terjadi kesalahan:', error);\n      });\n    },\n    confirmDelete(penyewaId) {\n      console.log(`Penyewa dengan ID ${penyewaId} akan dihapus`);\n    }\n  }\n};","map":{"version":3,"names":["axios","data","api","penyewa","searchQuery","created","getPenyewa","computed","filteredPenyewa","filter","searchTerm","toLowerCase","nama","includes","email","no_telp","methods","get","then","response","Array","isArray","console","error","catch","confirmDelete","penyewaId","log"],"sources":["D:\\SEMESTER 5\\Proyek\\Web_Kost\\frontend\\src\\views\\ListPenyewa.vue"],"sourcesContent":["<template>\r\n  <div class=\"content\">\r\n    <div class=\"row\">\r\n      <div class=\"col-md-12\">\r\n        <div class=\"card\">\r\n          <div class=\"card-header d-flex justify-content-between\">\r\n            <h4 class=\"card-title\">Daftar Penyewa</h4>\r\n          </div>\r\n          <div class=\"card-body\">\r\n            <div class=\"d-flex justify-content-between mb-3\">\r\n              <router-link to=\"/tambah-penyewa\">\r\n                <button class=\"btn btn-purple btn-sm\">Tambah Data</button>\r\n              </router-link>\r\n              <input \r\n                v-model=\"searchQuery\" \r\n                type=\"text\" \r\n                class=\"form-control\" \r\n                placeholder=\"Cari penyewa...\" \r\n                style=\"width: 250px;\" \r\n              />\r\n            </div>\r\n            <div class=\"table-responsive\">\r\n              <table class=\"table\">\r\n                <thead class=\"text-primary\">\r\n                  <tr>\r\n                    <th>No</th>\r\n                    <th>Nama</th>\r\n                    <th>Email</th>\r\n                    <th>Phone</th>\r\n                    <th>Action</th>\r\n                  </tr>\r\n                </thead>\r\n                <tbody>\r\n                  <tr v-for=\"(penyewa, index) in filteredPenyewa\" :key=\"penyewa.id\">\r\n                    <td>{{ index + 1 }}</td>\r\n                    <td>{{ penyewa.nama }}</td>\r\n                    <td>{{ penyewa.email }}</td>\r\n                    <td>{{ penyewa.no_telp }}</td>\r\n                    <td class=\"text-right\">\r\n                      <router-link :to=\"'/edit-penyewa/' + penyewa.id\">\r\n                        <button class=\"btn btn-warning btn-sm\">Edit</button>\r\n                      </router-link>\r\n                      <button class=\"btn btn-danger btn-sm\" @click=\"confirmDelete(penyewa.id)\">Hapus</button>\r\n                    </td>\r\n                  </tr>\r\n                </tbody>\r\n              </table>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      api: 'http://127.0.0.1:5000/list-penyewa', \r\n      penyewa: [],\r\n      searchQuery: '' \r\n    };\r\n  },\r\n  created() {\r\n    this.getPenyewa();  \r\n  },\r\n  computed: {\r\n    filteredPenyewa() {\r\n      return this.penyewa.filter(penyewa => {\r\n        const searchTerm = this.searchQuery.toLowerCase();\r\n        return (\r\n          (penyewa.nama && penyewa.nama.toLowerCase().includes(searchTerm)) ||\r\n          (penyewa.email && penyewa.email.toLowerCase().includes(searchTerm)) ||\r\n          (penyewa.no_telp && penyewa.no_telp.toLowerCase().includes(searchTerm))\r\n        );\r\n      });\r\n    }\r\n  },\r\n  methods: {\r\n    getPenyewa() {\r\n      axios.get(this.api)\r\n        .then(response => {\r\n          if (Array.isArray(response.data)) {\r\n            this.penyewa = response.data; \r\n          } else {\r\n            console.error('Data yang diterima tidak sesuai:', response.data);\r\n          }\r\n        })\r\n        .catch(error => {\r\n          console.error('Terjadi kesalahan:', error);  \r\n        });\r\n    },\r\n    confirmDelete(penyewaId) {\r\n      console.log(`Penyewa dengan ID ${penyewaId} akan dihapus`);  \r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.table th, .table td {\r\n  text-align: center;\r\n}\r\n\r\n.table th {\r\n  background-color: #f8f9fa;\r\n}\r\n\r\n.btn-sm {\r\n  margin: 0 5px;\r\n}\r\n\r\n.btn-warning {\r\n  background-color: #ffc107;\r\n}\r\n\r\n.btn-danger {\r\n  background-color: #dc3545;\r\n}\r\n\r\n.btn-purple {\r\n  background-color: #75276d;\r\n  color: white;\r\n}\r\n\r\n.btn-purple:hover {\r\n  background-color: #5e1d57; \r\n}\r\n</style>\r\n"],"mappings":";;AAwDA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,GAAG,EAAE,oCAAoC;MACzCC,OAAO,EAAE,EAAE;MACXC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,UAAU,CAAC,CAAC;EACnB,CAAC;EACDC,QAAQ,EAAE;IACRC,eAAeA,CAAA,EAAG;MAChB,OAAO,IAAI,CAACL,OAAO,CAACM,MAAM,CAACN,OAAM,IAAK;QACpC,MAAMO,UAAS,GAAI,IAAI,CAACN,WAAW,CAACO,WAAW,CAAC,CAAC;QACjD,OACGR,OAAO,CAACS,IAAG,IA