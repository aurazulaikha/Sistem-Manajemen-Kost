reator-photo-img {\r\n    width: 60px;\r\n    height: 60px;\r\n    border-radius: 50%;\r\n    object-fit: cover;\r\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n  }\r\n  \r\n  .creator-details {\r\n    text-align: left;\r\n  }\r\n  \r\n  .creator-name {\r\n    font-size: 1.2rem;\r\n    font-weight: bold;\r\n    color: #333333;\r\n  }\r\n  \r\n  .creator-role {\r\n    font-size: 1rem;\r\n    color: #75276d;\r\n  }\r\n  </style>\r\n  "],"mappings":"AA2CE,eAAe;EACbA,IAAI,EAAE;AACR,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}